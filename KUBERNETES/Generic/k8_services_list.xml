  <command name="IMPORT">
    <operation><![CDATA[GET]]></operation>
    <post_template>{foreach $params.ingress as $ingress_index =&gt; $ing}
	{foreach $params.ports as $port_index =&gt; $port}
		{if $port.port == 443} 
			{assign_object_variable var=&quot;ingress.{$ingress_index}.endpoints.{$port_index}.link&quot; value=&quot;https://{$ing.hostname}&quot;}
		{else}
			{assign_object_variable var=&quot;ingress.{$ingress_index}.endpoints.{$port_index}.link&quot; value=&quot;http://{$ing.hostname}:{$port.port}&quot;}
		{/if}
	{/foreach}
{/foreach}
</post_template>
