  <command name="CREATE">
    <operation><![CDATA[!
router {$params.object_id} 1

{foreach $params.interfaces as $itf}
 interface {$itf.interface}
 {if isset($itf.isis_circuit_level)} circuit-type {$params.isis_circuit_level}
 {/if}
  {if isset($itf.isis_auth_key)}  hello-padding disable
   hello-password keychain  {$itf.isis_auth_key}
  {/if}
  {if isset($itf.isis_network)}  {$itf.isis_network}
  {/if}
  {if isset($itf.isis_csnp_interval) and isset($itf.isis_csnp_level)}  csnp-interval  {$itf.isis_csnp_interval} ($itf.isis_csnp_level)}  
  {/if}
  {foreach $itf.metric as $metric} 
    {if isset($metric.isis_metric_level) and isset($metric.isis_metric)}   address-family ipv4 unicast
    metric {$metric.isis_metric} {$metric.isis_metric_level}  
    !
    {/if}
    {if isset($metric.isis_metric_ipv6_level) and isset($metric.isis_ipv6_metric)}    address-family ipv6 unicast
    metric {$metric.isis_ipv6_metric} {$metric.isis_ipv6_metric_level}  
    !
    {/if}
  {/foreach}

{/foreach}

{if isset($params.isis_net)} net {$params.isis_net}
{/if}
{if isset($params.isis_is_level)} is-type {$params.isis_is_level}
{/if}
{if isset($params.isis_overload_bit)} {$params.isis_overload_bit} {if isset($params.isis_overload_bit_on_startup)} on-startup {$params.isis_overload_bit_on_startup} {/if} {if isset($params.isis_overload_bit_suppress)} suppress {$params.isis_overload_bit_suppress} {/if}
{/if}
{if isset($params.isis_max_lsp_lifetime)} max-lsp-lifetime {$params.isis_max_lsp_lifetime} 
{/if}
{if isset($params.isis_lsp_refresh_interval)} lsp-refresh-interval {$params.isis_lsp_refresh_interval} 
{/if}
{if isset($params.isis_lsp_gen_interval)} lsp-gen-interval{if isset($params.isis_lsp_gen_interval_level)} {$params.isis_lsp_gen_interval_level} {/if}{if isset($params.isis_lsp_gen_interval)} {$params.isis_lsp_gen_interval} {/if}{if isset($params.isis_lsp_gen_initial_wait)} {$params.isis_lsp_gen_initial_wait}{/if}{if isset($params.isis_prc_min_wait)} {$params.isis_prc_min_wait} 
{/if}
{/if}
{if isset($params.isis_log_adjacency_change)} log adjacency changes
{/if}
{if isset($params.isis_nfs_method)} nsf {$params.isis_nfs_method}
{/if}
{if isset($params.isis_vrf)} vrf-context {$params.isis_vrf}
{/if}


!
]]></operation>
