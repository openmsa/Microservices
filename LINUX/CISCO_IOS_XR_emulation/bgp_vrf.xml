  <command name="UPDATE">
    <operation><![CDATA[!
vrf {$params.vrf_name}
address-family ipv4 unicast
{foreach $params.route_targets as $rt}
    {if $rt.import_export == "import"}
        import route-target
        {$rt.route_target}
        !
    {/if}
{/foreach}
export route-policy {$params.route_map}
{foreach $params.route_targets as $rt}
    {if $rt.import_export == "export"}
        export route-target
        {$rt.route_target}
        !
    {/if}
{/foreach}
!
router bgp {$params.router_bgp}
  vrf {$params.vrf_name}
  rd {$params.rd}
  address-family ipv4 unicast
  redistribute {$params.bgp_vrf_redistribute}
  redistribute static route-policy {$params.bgp_vrf_redistrib_static_policy}
!]]></operation>
