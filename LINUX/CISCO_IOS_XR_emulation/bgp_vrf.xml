  <command name="CREATE">
    <operation><![CDATA[!
router bgp {$params.router_bgp}
  vrf {$params.vrf_name}
  rd {$params.rd}
  address-family ipv4 unicast
{if isset ($params.bgp_vrf_redistribute) && isset ($params.bgp_vrf_redistrib_static_policy)}
  redistribute {$params.bgp_vrf_redistribute}
  redistribute static route-policy {$params.bgp_vrf_redistrib_static_policy}
{/if}
  !
{if isset($params.neighbor)}
{foreach $params.neighbor as $neighbor}
{if isset($neighbor.bgp_ipv4_private_as)}neighbor {$neighbor.bgp_neighbor} {$neighbor.bgp_ipv4_private_as}
{/if}
{if isset($neighbor.bgp_ipv4_route_policy_in)}neighbor {$neighbor.bgp_neighbor} {$neighbor.bgp_ipv4_route_policy_in}
{/if}
{if isset($neighbor.bgp_ipv4_route_policy_out)}neighbor {$neighbor.bgp_neighbor} {$neighbor.bgp_ipv4_route_policy_out}
{/if}
{if isset($neighbor.bgp_vrf_maximum_prefix)}neighbor {$neighbor.bgp_neighbor} {$neighbor.bgp_vrf_maximum_prefix}
{/if}
{if isset($neighbor.bgp_ipv4_default_route)}neighbor {$neighbor.bgp_neighbor} {$neighbor.bgp_ipv4_default_route}
{/if}
{if isset($neighbor.bgp_vrf_neighbor_remote_as)}neighbor {$neighbor.bgp_neighbor} remote-as  {$neighbor.bgp_vrf_neighbor_remote_as}
{/if}
{if isset($neighbor.bgp_ipv4_activate)}neighbor {$neighbor.bgp_neighbor} {$neighbor.bgp_ipv4_activate}
{/if}
{if isset($neighbor.bgp_vrf_route_map_in)}neighbor {$neighbor.bgp_neighbor} {$neighbor.bgp_vrf_route_map_in}
{/if}
{if isset($neighbor.bgp_vrf_as_override)}neighbor {$neighbor.bgp_neighbor} {$neighbor.bgp_vrf_as_override}
{/if}
{if isset($neighbor.bgp_vrf_prefix_list_out)}neighbor {$neighbor.bgp_neighbor} {$neighbor.bgp_vrf_prefix_list_out}
{/if}
{if isset($neighbor.bgp_vrf_send_community)}neighbor {$neighbor.bgp_neighbor} {$neighbor.bgp_vrf_send_community}
{/if}
{if isset($neighbor.bgp_vrf_advertisement_interval)}neighbor {$neighbor.bgp_neighbor} {$neighbor.bgp_vrf_advertisement_interval}
{/if}
{if isset($neighbor.bgp_vrf_allow_as)}neighbor {$neighbor.bgp_neighbor} {$neighbor.bgp_vrf_allow_as}
{/if}
{if isset($neighbor.bgp_vrf_route_map_out)}neighbor {$neighbor.bgp_neighbor} {$neighbor.bgp_vrf_route_map_out}
{/if}
{/foreach}
{/if}
  !
!]]></operation>
