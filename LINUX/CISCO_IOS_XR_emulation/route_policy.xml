  <command name="CREATE">
    <operation><![CDATA[!
route-policy {$params.object_id}
!
{foreach $params.statements|@sortby:"priority" as $statements}
	{$continue=false}
	{* first iteration on blocks to look for potential "continue" *}
    {if isset($statements.block)}
    	{foreach $statements.block as $block}
	    	{if isset($block.continue) && !$statements@last}
	        	{$continue=true}
	        	{break}
	        {/if}
		{/foreach}
    {/if}

	{if $statements.action == "deny" && !$statements@last}
	{continue}
	{/if}
{*	ACTION: {$statements.action}   
	FIRST: {$statements@first} LAST: {$statements@last} INDEX: {$statements@index} TOTAL: {$statements@total}*}
    {$keys = array_keys($statements)}
    {$clean_keys=[]}
    {$idx=0}
    {* clean up the list of keys *}
    {foreach $keys as $key}
        {if $key == "block" || $key == "_order" || $key == "priority" || $key == "action"}
    		{continue}
    	{/if}
    	{$clean_keys[$idx]=$key}
    	{$idx=$idx+1}
    {/foreach}
    {$first=true}
    {$last=false}
    {foreach $clean_keys as $key}
    	{if $key == "block" || $key == "_order" || $key == "priority" || $key == "action"}
    		{continue}
    	{/if}
		{if $statements@first && $first}
if {else if ($statements@total>1 && !$statements@first && $first)} elseif {/if} {if $continue==true} not {/if} {if $key == "as_path"}	{if $first==false} and {else} {$first=false} {/if} (as-path in {$statements.as_path}) 	{else if $key == "community"} 	{$first_com=true} 	{if $first==false} and {else} {$first=false} {/if} ({foreach $statements.community as $commset} {if $first_com==false} and {else} {$first_com=false} {/if} community matches-any {$commset.community_set} {/foreach}) 	{else if $key == "destination"} {if $first==false} and {else} {$first=false} {/if} (destination in {$statements.destination}) 	{else if $key == "tag"} {if $first==false} and {else} {$first=false} {/if} (tag eq {$statements.tag}) {/if} {if $key@last}then 
	{*new line*}
		{/if}
	{*new line*}
    {/foreach}
    {if isset($statements.block)}
    	{*CR to keep*}
    	
        {foreach $statements.block as $block}
            {if isset($block.community)}
    	        {foreach $block.community as $comm_set}
set community {$comm_set.community_set}
	        	{/foreach}
            {/if}
			{if isset($block.extcommunity) && isset($block.extcommunity_type)} 
set extcommunity {$block.extcommunity_type} {$block.extcommunity}
            {/if}
            {if isset($block.local_preference)} 
set local-preference {$block.local_preference}
            {/if}
            {if isset($block.weight)}
set weight {$block.weight}
            {/if}
            {if isset($block.continue)}
drop
            {/if}
        {/foreach}
    {else}
    	{if !$statements@last}
pass
    	{/if}
    {/if}
    {if $statements@last}
	else
		{if $statements.action == "permit"}
		pass
		{else}
		drop
		{/if}
	endif
	{/if}
{/foreach}
  !
  
!
end-policy
!
]]></operation>
