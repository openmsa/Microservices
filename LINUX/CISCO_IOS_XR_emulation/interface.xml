  <command name="IMPORT">
    <operation><![CDATA[cat /config/cisco_ios_xr/ubiqube-10-30-19-214.conf]]></operation>
    <post_template>{if isset($params.validated) &amp;&amp; $params.validated == &quot;!&quot;} 
      {assign_object_variable var=&quot;validated&quot; value=true}
{else}
      {assign_object_variable var=&quot;validated&quot; value=false}
{/if}
{assign_object_variable var=&quot;link&quot; value=&quot;/opt/fmc_repository/Datafiles/{$SD-&gt;SDID}.interface.html&quot;}</post_template>
    <parser>
      <section>
        <regexp>@interface\s+(?&lt;object_id&gt;\S+)@</regexp>
        <xpath/>
      </section>
      <lines>
        <line>
          <array name="addresses">
            <regexp>@^\s+ipv4\s+address\s+(?&lt;ipv4_address&gt;\S+)\s+(?&lt;ipv4_mask&gt;\S+)\s*(?&lt;ipv4_type&gt;secondary)?$@</regexp>
          </array>
          <regexp/>
          <xpath/>
        </line>
        <line>
          <array/>
          <regexp>@^\s+description\s+(?&lt;int_description&gt;.*)@</regexp>
          <xpath/>
        </line>
        <line>
          <array/>
          <regexp>@^\s+mtu\s+(?&lt;mtu_int&gt;\d+)@</regexp>
          <xpath/>
        </line>
        <line>
          <array/>
          <regexp>@^\s+ipv6\s+address\s+(?&lt;ipv6_address&gt;[^/]+)/(?&lt;ipv6_prefix&gt;\d{1,3})@</regexp>
          <xpath/>
        </line>
        <line>
          <array/>
          <regexp>@^\s+service-policy\s+output\s+(?&lt;service_policy_out&gt;.*)@</regexp>
          <xpath/>
        </line>
        <line>
          <array/>
          <regexp>@^\s+service-policy\s+input\s+(?&lt;service_policy_in&gt;.*)@</regexp>
          <xpath/>
        </line>
        <line>
          <array/>
          <regexp>@^\s*(?&lt;validated&gt;!)@</regexp>
          <xpath/>
        </line>
        <line>
          <array/>
          <regexp>@^.*vrf\s+(?&lt;vrf_name&gt;.*)@</regexp>
          <xpath/>
        </line>
        <line>
          <array/>
          <regexp>@^\s+ipv4\s+verify\s+(?&lt;ip_urpf&gt;.+)@</regexp>
          <xpath/>
        </line>
        <line>
          <array name="encapsulation">
            <regexp>@^\s+encapsulation\s+dot1q\s+(?&lt;encapsulation_dot1q&gt;\d+)\s*(second-dot1q\s+(?&lt;encapsulation_second_dot1q&gt;\d+))?@</regexp>
          </array>
          <regexp/>
          <xpath/>
        </line>
        <line>
          <array/>
          <regexp>@^\s*ipv6\s+(?&lt;ipv6_enable&gt;enable)$@</regexp>
          <xpath/>
        </line>
        <line>
          <array name="carrier_delay">
            <regexp>@^\s+carrier-delay\s+msec\s+(?&lt;carrier_delay_msec&gt;\d+)@</regexp>
            <regexp>@^\s+carrier-delay\s+up\s+(?&lt;carrier_delay_up&gt;\d+)@</regexp>
            <regexp>@^\s+carrier-delay\s+down\s+msec\s+(?&lt;carrier_delay_down_msec&gt;\d+)@</regexp>
          </array>
          <regexp/>
          <xpath/>
        </line>
        <line>
          <array/>
          <regexp>@^\s+speed\s+(?&lt;int_speed&gt;\d+)@</regexp>
          <xpath/>
        </line>
        <line>
          <array/>
          <regexp>@^\s+duplex\s+(?&lt;duplex_mode&gt;\S+)@</regexp>
          <xpath/>
        </line>
