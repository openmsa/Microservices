  <command name="CREATE">
    <operation><![CDATA[!
interface {$params.object_id}
{if isset($params.int_description) }description {$params.int_description}
{/if}
{foreach $params.addresses as $addr}
{if isset($addr.ipv4_address) && isset($addr.ipv4_mask) }
{if isset($addr.ipv4_type)}
ipv4 address {$addr.ipv4_address} {$addr.ipv4_mask} {$addr.ipv4_type}
{else if}
ipv4 address {$addr.ipv4_address} {$addr.ipv4_mask}
{/if}
{/if}
{/foreach}
{if isset($params.ipv6_address) }
ipv6 address {$params.ipv6_address}/{$params.ipv6_prefix}
{/if}
{if isset($params.ipv6_enable) }ipv6 {$params.ipv6_enable}
{/if}
{if isset($params.vrf_name) }vrf {$params.vrf_name}
{/if}
{if isset($params.mtu_int) }mtu {$params.mtu_int}
{/if}
!]]></operation>
  </command>
  <command name="READ">
    <operation><![CDATA[<pre>
!
interface {$interface.$object_id.object_id}
{if isset($interface.$object_id.int_description) }description {$interface.$object_id.int_description}
{/if}
{foreach $interface.$object_id.addresses as $addr}
{if isset($addr.ipv4_address) && isset($addr.ipv4_mask) }
{if isset($addr.ipv4_type)}
ipv4 address {$addr.ipv4_address} {$addr.ipv4_mask} {$addr.ipv4_type}
{else if}
ipv4 address {$addr.ipv4_address} {$addr.ipv4_mask}
{/if}
{/if}
{/foreach}
{if isset($interface.$object_id.ipv6_address) }
ipv6 address {$interface.$object_id.ipv6_address}/{$interface.$object_id.ipv6_prefix}
{/if}
{if isset($interface.$object_id.ipv6_enable) }ipv6 {$interface.$object_id.ipv6_enable}
{/if}
{if isset($interface.$object_id.vrf_name) }vrf {$interface.$object_id.vrf_name}
{/if}
{if isset($interface.$object_id.mtu_int) }mtu {$interface.$object_id.mtu_int}
{/if}
!
</pre>]]></operation>
