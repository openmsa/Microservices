  <command name="IMPORT">
    <operation><![CDATA[cat /config/cisco_ios/sample-config.ios.route_map.txt]]></operation>
    <post_template>{assign var=filtered_route_map value=[]} 
{assign var=route_map_idx value=0}

{foreach $route_map as $rm}
    {if $params.object_id == $rm.name}
        MATCH rp object_id: {$params.object_id} -&gt; rm name: {$rm.name}
        {$filtered_route_map[$route_map_idx]=$rm}
    {/if}
    {$route_map_idx=$route_map_idx+1}
{/foreach}

{$statement_idx=0}
{if isset($filtered_route_map)}
{foreach $filtered_route_map as $rm}
    {if $rm@first}
        {assign_object_variable  
        var=&quot;statements.{$statement_idx}.statement&quot; 
        value=&quot;if&quot;}
    {else if $rm@last}
        {assign_object_variable 
        var=&quot;statements.{$statement_idx}.statement&quot; 
        value=&quot;else&quot;}
    {else}
        {assign_object_variable  
        var=&quot;statements.{$statement_idx}.statement&quot; 
        value=&quot;elseif&quot;}
    {/if}
    
    {if isset($rm.matches)}
    {foreach $rm.matches as $match}

    {if isset($match.interface)}
        {assign_object_variable   
        var=&quot;statements.{$statement_idx}.interface&quot;
        value=&quot;{$match.interface}&quot;}
    {/if}
    {if isset($match.tag)}
        {assign_object_variable   
        var=&quot;statements.{$statement_idx}.tag&quot;
        value=&quot;{$match.tag}&quot;}
    {/if}

    {if isset($match.ip_prefix_list)}
        {assign_object_variable   
        var=&quot;statements.{$statement_idx}.prefix_set&quot;
        value=&quot;{$match.ip_prefix_list}&quot;}
    {/if}
   
    {/foreach}
    {/if}
    
    {$block_idx=0}
    {if isset($rm.operator)}
        {assign_object_variable  var=&quot;statements.{$statement_idx}.block.{$block_idx}.operator&quot;
        value=&quot;{$rm.operator}&quot;}
        {$block_idx=$block_idx+1}
    {/if}
    {if isset($rm.weight)}
        {assign_object_variable  var=&quot;statements.{$statement_idx}.block.{$block_idx}.weight&quot;
        value=&quot;{$rm.weight}&quot;}
        {$block_idx=$block_idx+1}{/if}
    {if isset($rm.local_preference)}
        {assign_object_variable  var=&quot;statements.{$statement_idx}.block.{$block_idx}.local_preference&quot;
        value=&quot;{$rm.local_preference}&quot;}
        {$block_idx=$block_idx+1}
    {/if}
    {if isset($rm.extcommunity)}

        {foreach $rm.extcommunity as $extcomm}
{assign_object_variable 
var=&quot;statements.{$statement_idx}.block.{$block_idx}.extcommunity&quot;
value=&quot;{$extcomm.extcommunity}&quot;}
            {if !$extcomm@first}
                {assign_object_variable  var=&quot;statements.{$statement_idx}.block.{$block_idx}.additive&quot;
                    value=&quot;additive&quot;}
            {/if}
            {$block_idx=$block_idx+1}
        {/foreach}
    {/if}
    
    {$statement_idx=$statement_idx+1}
{/foreach}
{/if}</post_template>
