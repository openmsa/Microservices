  <example/>
  <command name="IMPORT">
    <operation><![CDATA[cat /config/cisco_ios/sample-config.ios.txt]]></operation>
    <post_template>{if isset($params.timeslots) &amp;&amp; $params.timeslots}
	{$bd=64}
	{if $params.timeslots == &quot;1-8&quot;}
		{$bd=$bd*8}
	{else if $params.timeslots == &quot;1-16&quot;}
		{$bd=$bd*16}
	{else if $params.timeslots == &quot;1-31&quot;}
		{$bd=$bd*31}
	{/if}
	{assign_object_variable
		var=&quot;bandwidth&quot;
		value=&quot;{$bd}&quot;}
{/if}

{foreach $params.mode as $mode}
	{foreach $mode.timeslots_array as $timeslots}
			{assign_object_variable
			var=&quot;mode.{$mode@index}.timeslots_array.{$timeslots@index}.sub_interface&quot;
			value=&quot;{$mode.au_num}/{$mode.tug_num}/{$timeslots.tug_num}/{$timeslots.e_num}:{$timeslots.channel_group}&quot;}

		{if isset($timeslots.timeslots) &amp;&amp; $timeslots.timeslots}
			{$bd=64}
		{if $timeslots.timeslots == &quot;1-8&quot;}
			{$bd=$bd*8}
		{else if $timeslots.timeslots == &quot;1-16&quot;}
			{$bd=$bd*16}
		{else if $timeslots.timeslots == &quot;1-31&quot;}
			{$bd=$bd*31}
		{/if}
		{assign_object_variable
			var=&quot;mode.{$mode@index}.timeslots_array.{$timeslots@index}.bandwidth&quot;
			value=&quot;{$bd}&quot;}
		{/if}
	{/foreach}
{/foreach}</post_template>
