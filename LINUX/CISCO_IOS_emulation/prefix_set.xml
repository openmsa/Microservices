        <regexp>@^\s*ip\s+access-list\s+(extended|standard)\s+(?&lt;object_id&gt;\S+)@</regexp>
        <regexp>@^\s*access-list\s+(?&lt;object_id&gt;\d+)@</regexp>
        <regexp>@^\s*(ip|ipv6)\s+prefix-list\s+(?&lt;object_id&gt;\S+)@</regexp>
      </section>
      <lines>
