  <command name="IMPORT">
    <operation>vcenter</operation>
    <post_template>{assign_object_variable var=&quot;display_var&quot; value=&quot;{$params.name} - {$params.object_id} - {$params.cpu_count} - {$params.memory_size_MiB} - {$params.power_state}&quot;}</post_template>
    <parser>
      <section>
        <regexp>@^\s \s*$@</regexp>
        <xpath>//root/value/row</xpath>
      </section>
      <lines>
        <line>
          <array/>
          <regexp>@&lt;name&gt;(?&lt;name&gt;.*)&lt;/name&gt;@</regexp>
          <xpath>name</xpath>
        </line>
        <line>
          <array/>
          <regexp>@&lt;vm&gt;(?&lt;object_id&gt;.*)&lt;/vm&gt;@</regexp>
          <xpath>vm</xpath>
        </line>
        <line>
          <array/>
          <regexp>@&lt;cpu_count&gt;(?&lt;cpu_count&gt;.*)&lt;/cpu_count&gt;@</regexp>
          <xpath>cpu_count</xpath>
        </line>
        <line>
          <array/>
          <regexp>@&lt;memory_size_MiB&gt;(?&lt;memory_size_MiB&gt;.*)&lt;/memory_size_MiB&gt;@</regexp>
          <xpath>memory_size_MiB</xpath>
        </line>
        <line>
          <array/>
          <regexp>@&lt;power_state&gt;(?&lt;power_state&gt;.*)&lt;/power_state&gt;@</regexp>
          <xpath>power_state</xpath>
        </line>
      </lines>
    </parser>
    <xpath>vm</xpath>
  </command>
  <command name="UPDATE">
    <operation>{}</operation>
    <rest>POST#vcenter</rest>
    <xpath>vm/{$params.object_id}/power/{if $params.power_state === &quot;POWERED_ON&quot; }start{elseif $params.power_state === &quot;POWERED_OFF&quot;}stop{elseif $params.power_state === &quot;POWERED_SUSPEND&quot;}suspend{else}reset{/if}</xpath>
  </command>
  <command name="CONSTRAINT"/>
  <command name="CREATE"/>
  <command name="DELETE">
    <operation>{}</operation>
    <rest>vcenter</rest>
    <xpath>vm/{$vm.$object_id.object_id}</xpath>
  </command>
  <command name="READ"/>
  <command name="LIST"/>
