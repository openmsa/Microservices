  <command name="CREATE">
    <operation>{
&quot;spec&quot;: {
&quot;name&quot;: &quot;{$params.name}&quot;,
&quot;powered_on&quot;: true,
&quot;vm_home_storage&quot;: {
&quot;datastore&quot;: &quot;{$params.datastore}&quot;,
&quot;storage_policy&quot;: {
&quot;type&quot;: &quot;USE_SOURCE_POLICY&quot;
}
},
&quot;placement&quot;: {
&quot;host&quot;: &quot;{$params.host}&quot;,
&quot;folder&quot;:&quot;{$params.folder}&quot;
},
&quot;hardware_customization&quot;: {
&quot;cpu_update&quot;: {
&quot;num_cpus&quot;: {$params.cpu_count}
},
&quot;memory_update&quot;: {
&quot;memory&quot;: {$params.memory}
},
&quot;nics&quot;: [ {{assign var=&quot;i&quot; value=0}}
{foreach $params.networks as $item}
{if $i &gt; 0},{/if}{
&quot;key&quot;: &quot;400{$i}&quot;,
&quot;value&quot;: {
&quot;network&quot;: &quot;{$item.network}&quot;
}
}
{assign var=&quot;i&quot; value=$i+1}
{/foreach}
]
}
}}</operation>
    <rest>POST#vcenter</rest>
    <xpath>vm-template/library-items/{$params.template_id}?action=deploy</xpath>
  </command>
