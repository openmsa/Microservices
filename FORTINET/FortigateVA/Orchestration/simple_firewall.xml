  <command name="CREATE">
    <operation><![CDATA[config firewall address
edit "{$params.src_ip}"
set type ipmask
set subnet {$params.src_ip} {$params.src_mask}
next
end

config firewall address
edit "{$params.dst_ip}"
set type ipmask
set subnet {$params.dst_ip} {$params.dst_mask}
next
end

config firewall policy
edit {$params.object_id}
set name "{$params.object_id}"
set srcintf {$params.src_itf}
set dstintf {$params.dst_itf}
set srcaddr "{$params.src_ip}"
set match-vip disable
set dstaddr "{$params.dst_ip}"
set status enable
set schedule always
set service "{$params.service}"
set action {$params.action}
set logtraffic all
{if $params.action == accept}
set utm-status disable
set nat disable
{/if}
next
end
]]></operation>
