  <example>
    <content>FGTAWS0003E6099A # show full-configuration log syslogd3 setting
config log syslogd3 setting
set status enable
set server &quot;91.167.210.90&quot;
set mode udp
set port 514
set facility local7
set source-ip &apos;&apos;
set format default
end</content>
  </example>
  <command name="IMPORT">
    <operation><![CDATA[show full-configuration log syslogd setting]]></operation>
    <post_template>{if isset($params.syslogd_server_ip) &amp;&amp; $params.syslogd_server_ip != &quot;&quot;}
      {assign_object_variable var=&quot;_syslogd_server_ip&quot; value=&quot;{$params.syslogd_server_ip}&quot;}
{/if}
</post_template>
    <parser>
      <section>
        <regexp>@config log (?&lt;object_id&gt;\S+) setting@</regexp>
        <xpath></xpath>
      </section>
      <lines>
        <line>
          <array/>
          <regexp>@\s*set status (?&lt;syslogd_status&gt;\S+)@</regexp>
          <xpath/>
        </line>
        <line>
          <array/>
          <regexp>@\s*set port (?&lt;syslogd_port&gt;\d+)@</regexp>
          <xpath/>
        </line>
        <line>
          <array/>
          <regexp>@\s*set server &quot;(?&lt;syslogd_server_ip&gt;[^&quot;]+)&quot;@</regexp>
          <xpath/>
        </line>
        <ignore>
          <regexp>@set@</regexp>
        </ignore>
      </lines>
    </parser>
  </command>
  <command name="UPDATE">
    <operation><![CDATA[config log syslogd setting
{if $params.syslogd_status=="enable"}
set status enable
{if !empty($params.syslogd_facility_name)}
set facility {$params.syslogd_facility_name}
{/if}
{if !empty($params.syslogd_port)}
set port {$params.syslogd_port}
{else}
set port 514
{/if}
set server {$params.syslogd_server_ip}
{else}
set status disable
{/if}
end]]></operation>
  </command>
