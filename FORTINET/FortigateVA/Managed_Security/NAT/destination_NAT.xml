  <command name="IMPORT">
    <operation><![CDATA[show full-configuration firewall vip]]></operation>
    <post_template>{if ($params.statusPortForward==&quot;disable&quot;) }
{assign_object_variable var=&quot;portFor&quot; value=&quot;false&quot;}
{else}
