  <command name="IMPORT">
    <operation><![CDATA[show full-configuration system interface##get system interface]]></operation>
    <post_template>{if empty($params.alias)}
{assign_object_variable var=&quot;alias&quot; value=$params.object_id}
{/if}</post_template>
    <parser>
      <section>
        <regexp>@edit &quot;(?&lt;object_id&gt;(port[1-9]|port10))&quot;@</regexp>
        <regexp>@^name: (?&lt;object_id&gt;(port[1-9]|port10)).*ip: (?&lt;addressIP&gt;\S+) (?&lt;mask&gt;\S+)@</regexp>
      </section>
      <lines>
        <line>
          <array/>
          <regexp>@set ip (?&lt;addressIP&gt;\S+) (?&lt;mask&gt;\S+)@</regexp>
          <xpath/>
        </line>
        <line>
          <array/>
          <regexp>@set status (?&lt;status&gt;\S+)@</regexp>
          <xpath/>
        </line>
        <line>
          <array/>
          <regexp>@set description &quot;(?&lt;comment&gt;[^&quot;]+)&quot;@</regexp>
          <xpath/>
        </line>
        <line>
          <array/>
          <regexp>@set alias &quot;(?&lt;alias&gt;[^&quot;]+)&quot;@</regexp>
          <xpath/>
        </line>
        <line>
          <array/>
          <regexp>@set mode (?&lt;mode&gt;\S+)@</regexp>
          <xpath/>
        </line>
        <ignore>
          <regexp>@set @</regexp>
        </ignore>
        <ignore>
          <regexp>@config @</regexp>
        </ignore>
        <ignore>
          <regexp>@unset @</regexp>
        </ignore>
        <ignore>
          <regexp>@next@</regexp>
        </ignore>
        <ignore>
          <regexp>@end@</regexp>
        </ignore>
      </lines>
    </parser>
  </command>
  <command name="UPDATE">
    <operation><![CDATA[config system interface
edit "{$params.object_id}"
{if empty($params.addressIP) && empty($params.mask)}
unset ip
{else}
set ip {$params.addressIP} {$params.mask}
{/if}
set mtu-override enable
set ident-accept enable
set status {$params.status}
{if empty($params.comment)}
unset description
{else}
set description "{$params.comment}"
{/if}
end
]]></operation>
