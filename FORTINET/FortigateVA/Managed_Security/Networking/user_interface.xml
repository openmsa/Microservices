          <array/>
          <regexp>@set ip (?&lt;addressIP>\S+) (?&lt;mask>\S+)@</regexp>
        </line>
        <line>
          <array/>
          <regexp>@set allowaccess (?&lt;verifPing>ping)@</regexp>
        </line>
        <line>
