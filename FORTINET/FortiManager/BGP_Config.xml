  <command name="CREATE">
    <operation><![CDATA[{ldelim}
"method": "add",
"params": [
{ldelim}
"data": 
[
{ldelim}
"description": "BGP Config template",
"name": "{$params.BGP_temp_name}",
"script": "config router bgp\nset as {$params.AS_no}\nset router-id {$params.routerid}\nset ebgp-multipath enable\nset graceful-restart enable\nconfig neighbor\nedit {$params.neighip1}\nset advertisement-interval 1\nset ebgp-enforce-multihop enable\nset link-down-failover enable\nset soft-reconfiguration enable\nset remote-as {$params.remote_AS_no1}\n
set route-map-in {$params.route_map_in}\nset route-map-out {$params.route_map_out1}\nset keep-alive-timer 1\nset holdtime-timer 3\nnext\nedit {$params.neighip2}\nset advertisement-interval 1\nset ebgp-enforce-multihop enable\nset soft-reconfiguration enable\nset remote-as {$params.remote_AS_no2}\nset route-map-out {$params.route_map_out2}\nset keep-alive-timer 1\nset holdtime-timer 3\nnext\nedit {$params.neighip3}\nset advertisement-interval 1\nset ebgp-enforce-multihop enable\nset soft-reconfiguration enable\nset remote-as {$params.remote_AS_no3}\nset route-map-out {$params.route_map_out3}\nset keep-alive-timer 1\nset holdtime-timer 3\nnext\nend\nconfig network\nedit {$params.network_no1}\nset prefix {$params.network_ip1} {$params.network_subnetmask1}\nnext\nedit {$params.network_no2}\nset prefix {$params.network_ip2} {$params.network_subnetmask2}\nnext\nedit {$params.network_no3}\nset prefix {$params.network_ip3} {$params.network_subnetmask2}\nnext\nedit {$params.network_no4}\nset prefix {$params.network_ip4} {$params.network_subnetmask2}\nnext\nedit {$params.network_no5}\nset prefix {$params.network_ip5} {$params.network_subnetmask1}\nnext\nend\nend\n"
{rdelim}
],
"url":"/pm/config/adom/{$ADOM}/obj/cli/template"
{rdelim}
],
"id": 1
{rdelim}]]></operation>
