  <command name="IMPORT">
    <operation><![CDATA[get system interface]]></operation>
    <parser>
      <section>
        <regexp>@==\s+\[\s+(?&lt;object_id&gt;port\d+)@</regexp>
      </section>
      <lines>
        <line>
          <array/>
          <regexp>@.ip:.(?&lt;address&gt;\S*)@</regexp>
          <xpath/>
        </line>
        <line>
          <array/>
          <regexp>@status:.(?&lt;status&gt;\S*)@</regexp>
          <xpath/>
        </line>
        <line>
          <array/>
          <regexp>@description:.(?&lt;comment&gt;.*)@</regexp>
          <xpath/>
        </line>
        <ignore>
          <regexp>@.type:@</regexp>
        </ignore>
        <ignore>
          <regexp>@.ip6:@</regexp>
        </ignore>
        <ignore>
          <regexp>@.allowaccess:@</regexp>
        </ignore>
        <ignore>
          <regexp>@.mode@</regexp>
        </ignore>
        <ignore>
          <regexp>@.ip6-mode@</regexp>
        </ignore>
      </lines>
    </parser>
  </command>
  <command name="UPDATE">
    <operation><![CDATA[config system interfaceedit "{$params.object_id}"set type physicalset ip6 ::/0unset allowaccessset status {$params.status}set mode dhcpset ip6-mode static{if empty($params.comment)}unset description{else}set description {$params.comment}{/if}configsecondaryipendunset ip6-allowaccessset wccp disableset mtu 1500nextend
]]></operation>
  </command>
