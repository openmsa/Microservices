  <command name="IMPORT">
    <operation>show waf signature</operation>
    <post_template>{$sig_dis_ref=$signatures_disabled[$params.object_id]}
{$sig_index=0}{if !empty($sig_dis_ref.dis)}
{foreach $sig_dis_ref.dis as $sig_dis}{assign_object_variable var="sig_disabled_tab.{$sig_index}.id" value=$sig_dis.id}{assign var="sig_index" value=$sig_index+1}
{/foreach}
{/if}{$sig_alert_ref=$signatures_alert_only[$params.object_id]}
{$sig_alert_index=0}{if !empty($sig_alert_ref.dis)}
{foreach $sig_alert_ref.dis as $sig_dis}{assign_object_variable var="sig_alert_only_tab.{$sig_alert_index}.id" value=$sig_dis.id}{assign var="sig_alert_index" value=$sig_alert_index+1}
{/foreach}
{/if}{$sig_main_ref=$signatures_main[$params.object_id]}
{$sig_main_index=0}{if !empty($sig_main_ref.dis)}
{foreach $sig_main_ref.dis as $sig_dis}{if $sig_dis.status == "disable"}{assign_object_variable var="sig_main_tab.{$sig_main_index}.id1" value=$sig_dis.id}{assign_object_variable var="sig_main_tab.{$sig_main_index}.status1" value=$sig_dis.status}{assign_object_variable var="sig_main_tab.{$sig_main_index}.sta" value="false"}{else}{assign_object_variable var="sig_main_tab.{$sig_main_index}.id" value=$sig_dis.id}{assign_object_variable var="sig_main_tab.{$sig_main_index}.status" value="enable"}{assign_object_variable var="sig_main_tab.{$sig_main_index}.st" value="true"}{/if}{assign_object_variable var="sig_main_tab.{$sig_main_index}.action" value=$sig_dis.action}{assign_object_variable var="sig_main_tab.{$sig_main_index}.block_period" value=$sig_dis.block_period}{assign_object_variable var="sig_main_tab.{$sig_main_index}.severity" value=$sig_dis.severity}{assign var="sig_main_index" value=$sig_main_index+1}
{/foreach}
{/if}</post_template>
