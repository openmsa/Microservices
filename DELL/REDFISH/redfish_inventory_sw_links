<command name="IMPORT">
    <operation>GET</operation>
    <xpath>/redfish/v1/UpdateService/SoftwareInventory</xpath>
    <parser>
      <section>
        <xpath>//root/Members/row</xpath>
      </section>
      <lines>
        <line>
          <xpath>//ATodataDOTid</xpath>
          <regexp>@&lt;ATodataDOTid&gt;(?&lt;object_id&gt;.*)&lt;/ATodataDOTid&gt;@</regexp>
        </line>
      </lines>
    </parser>
    <post_template>{assign var="link_array" value='/'|explode:$params.object_id}
{assign_object_variable var="system" value=$link_array|@end}</post_template>
  </command>
<command name="UPDATE">
    <rest>
</rest>
    <xpath>
</xpath>
    <operation>&lt;system xmlns="http://oneaccess-net.com/oneos/glob"&gt;    
  &lt;class-map xmlns="http://oneaccess-net.com/oneos/ipservices/qos"&gt;
    &lt;name&gt;{$params.object_id}&lt;/name&gt;
    {foreach $params.rule_list item=rule}
    &lt;match&gt;
      {if $rule.type eq 'dscp'}
      &lt;matchRule&gt;ip dscp {$rule.value}&lt;/matchRule&gt;
      {elseif}
      

      &lt;not/&gt;
    &lt;/match&gt;
  &lt;/class-map&gt;
&lt;/system&gt;
</operation>
