  <command name="IMPORT">
    <operation>sh run | section interface</operation>
    <post_template>{if !empty($params.vlanNumber)}
{assign_object_variable var=&quot;vlan&quot; value=&quot;true&quot;}
{else}
{assign_object_variable var=&quot;vlan&quot; value=&quot;false&quot;}
{/if}</post_template>
    <parser>
      <section>
        <regexp>@^interface (?&lt;object_id&gt;.*)@</regexp>
        <xpath/>
      </section>
      <lines>
        <line>
          <array/>
          <regexp>@\s+description (?&lt;description&gt;.*)@</regexp>
          <xpath/>
        </line>
        <line>
          <array/>
          <regexp>@\s+switchport access vlan (?&lt;vlanNumber&gt;\d+)@</regexp>
          <xpath/>
        </line>
        <line>
          <array/>
          <regexp>@\s+ip address (?&lt;ip&gt;\S+) (?&lt;mask&gt;\S+)@</regexp>
          <xpath/>
        </line>
        <ignore>
          <regexp>@^\s+\S+.*$@</regexp>
        </ignore>
        <ignore>
          <regexp>@^\s*$@</regexp>
        </ignore>
        <ignore>
          <regexp>@^!.*$@</regexp>
        </ignore>
      </lines>
    </parser>
  </command>
  <command name="UPDATE">
    <operation>interface {$params.object_id}
no description{if !empty($params.description)}description {$params.description}{/if}
no switchport mode trunk
no switchport access vlan{if $params.vlan === &quot;true&quot;}switchport access vlan {$params.vlanNumber}{/if}no snmp trap link-statusno cdp enable</operation>
  </command>
