  <command name="CREATE">
    <operation><![CDATA[{if isset($params.vrf_name) && isset($params.router_bgp)}
!
router bgp {$params.router_bgp}
 vrf {$params.vrf_name}
 {if isset($params.rd)}
  rd {$params.rd}
 {/if}
{if isset($params.bgp_vrf_redistribute) 
    || isset ($params.bgp_vrf_redistrib_static_policy) 
    || isset($params.neighbor)}
  
{if isset ($params.bgp_vrf_redistribute)}
  address-family ipv4 unicast redistribute {$params.bgp_vrf_redistribute}
{/if}
{if isset($params.bgp_vrf_redistrib_static_policy)}
  address-family ipv4 unicast redistribute static route-policy {$params.bgp_vrf_redistrib_static_policy}
{/if}
{if isset($params.bgp_dampening)}
  address-family ipv4 unicast {$params.bgp_dampening}
{/if}
 !
{if isset($params.neighbor)}
{foreach $params.neighbor as $neighbor}

{if isset($neighbor.bgp_vrf_neighbor_remote_as)} neighbor {$neighbor.bgp_vrf_neighbor} remote-as  {$neighbor.bgp_vrf_neighbor_remote_as}
{/if}
{if isset($neighbor.bgp_vrf_neighbor_password)} neighbor {$neighbor.bgp_vrf_neighbor} password encrypted {$neighbor.bgp_vrf_neighbor_password}
{/if}
{if isset($neighbor.bgp_vrf_as_override)} neighbor {$neighbor.bgp_vrf_neighbor} address-family ipv4 unicast {$neighbor.bgp_vrf_as_override}
{/if}
{if isset($neighbor.bgp_ipv4_route_policy_in)} neighbor {$neighbor.bgp_vrf_neighbor} address-family ipv4 unicast  route-policy {$neighbor.bgp_ipv4_route_policy_in} in
{/if}
{if isset($neighbor.bgp_ipv4_route_policy_out)} neighbor {$neighbor.bgp_vrf_neighbor} address-family ipv4 unicast  route-policy {$neighbor.bgp_ipv4_route_policy_out} out
{/if}

{*
{if isset($neighbor.bgp_ipv4_private_as)} {$neighbor.bgp_ipv4_private_as}
{/if}
{if isset($neighbor.bgp_vrf_maximum_prefix)}neighbor {$neighbor.bgp_vrf_neighbor} {$neighbor.bgp_vrf_maximum_prefix}
{/if}
{if isset($neighbor.bgp_ipv4_default_route)}neighbor {$neighbor.bgp_vrf_neighbor} {$neighbor.bgp_ipv4_default_route}
{/if}
{if isset($neighbor.bgp_vrf_prefix_list_out)}neighbor {$neighbor.bgp_vrf_neighbor} {$neighbor.bgp_vrf_prefix_list_out}
{/if}
{if isset($neighbor.bgp_vrf_send_community)}neighbor {$neighbor.bgp_vrf_neighbor} {$neighbor.bgp_vrf_send_community}
{/if}
{if isset($neighbor.bgp_vrf_advertisement_interval)}neighbor {$neighbor.bgp_vrf_neighbor} {$neighbor.bgp_vrf_advertisement_interval}
{/if}
{if isset($neighbor.bgp_vrf_allow_as)}neighbor {$neighbor.bgp_vrf_neighbor} {$neighbor.bgp_vrf_allow_as}
{/if}
*}
{/foreach}
{/if}
{/if}
root
!
{/if}]]></operation>
  </command>
  <command name="READ"/>
  <command name="UPDATE">
    <operation><![CDATA[!
vrf {$params.vrf_name}
address-family ipv4 unicast
{foreach $params.route_targets as $rt}
    {if $rt.import_export == "import"}
        import route-target
        {$rt.route_target}
        !
    {/if}
{/foreach}
export route-policy {$params.route_map}
{foreach $params.route_targets as $rt}
    {if $rt.import_export == "export"}
        export route-target
        {$rt.route_target}
        !
    {/if}
{/foreach}
!
router bgp {$params.router_bgp}
  vrf {$params.vrf_name}
  rd {$params.rd}
  address-family ipv4 unicast
  redistribute {$params.bgp_vrf_redistribute}
  redistribute static route-policy {$params.bgp_vrf_redistrib_static_policy}
!]]></operation>
  </command>
  <command name="CONSTRAINT"/>
  <command name="DELETE">
    <operation><![CDATA[
{if isset($params.neighbor) && count($params.neighbor)>1}
!
router bgp {$bgp_vrf.$object_id.router_bgp}
 vrf {$params.vrf_name}
{foreach $params.neighbor as $neighbor}
    no neighbor {$neighbor.bgp_vrf_neighbor}
{/foreach}
root
!
{/if}]]></operation>
