  <command name="IMPORT">
    <operation><![CDATA[#cat /config/cisco_ios_xr/sample-config.ios-xr.V4815.conf]]></operation>
    <post_template>{$import_index=0}
{$export_index=0}
{foreach $params.import_rt.0.rt_array as $key=&gt;$value}
	{assign_object_variable var=&quot;import_array_rt.{$import_index}.import_rt&quot; value=&quot;{$value.import_rt}&quot;}
	{$import_index=$import_index+1}
{/foreach}
{foreach $params.import_rt.1.rt_array as $key=&gt;$value}
	{assign_object_variable var=&quot;export_array_rt.{$export_index}.export_rt&quot; value=&quot;{$value.import_rt}&quot;}
	{$export_index=$export_index+1}
{/foreach}
{foreach $router_bgp as $router}
	{assign_object_variable var=&quot;router_bgp&quot; value=&quot;{$router.router_bgp}&quot;}
{/foreach}
{assign_object_variable var=&quot;vrf_name&quot; value=&quot;{$params.object_id}&quot;}
</post_template>
    <parser>
      <section>
        <regexp>@^\s*vrf\s*(?&lt;object_id&gt;(?&lt;customer_prefix&gt;[^:]+)\S+)$@</regexp>
        <xpath></xpath>
      </section>
      <lines>
        <line>
          <array/>
          <regexp>@^\s*export\s+route-policy\s+(?&lt;route_map&gt;\S+)@</regexp>
          <xpath/>
        </line>
        <line>
          <array/>
          <regexp>@^\s*rd\s+(?&lt;rd&gt;[0-9]+:[0-9]+)@</regexp>
          <xpath/>
        </line>
        <line>
          <array/>
          <regexp>@^\s*description\s+(?&lt;description&gt;.*)$@</regexp>
          <xpath/>
        </line>
        <line>
          <array name="import_rt">
            <lines>
              <line>
                <array name="rt_array">
                  <regexp>@^\s*(?&lt;import_rt&gt;\d+:\d+)$@</regexp>
                </array>
                <regexp/>
                <xpath/>
              </line>
            </lines>
            <lines/>
            <regexp>@import route-target@</regexp>
            <regexp>@export route-target@</regexp>
          </array>
          <regexp/>
          <xpath/>
        </line>
        <line>
          <array/>
          <regexp>@^\s*address-family\s+(?&lt;bgp_address_family&gt;ipv4)\s+unicast$@</regexp>
          <xpath/>
        </line>
        <line>
          <array/>
          <regexp>@^\s*redistribute\s+(?&lt;bgp_vrf_redistribute&gt;static|connected)$@</regexp>
          <xpath/>
        </line>
        <line>
          <array/>
          <regexp>@^\s*redistribute\s+(?&lt;bgp_vrf_redistrib_policy&gt;static)\s+route-policy\s+(?&lt;bgp_vrf_redistrib_static_policy&gt;\S+)$@</regexp>
          <xpath/>
        </line>
        <ignore>
          <regexp>@^\s*!@</regexp>
        </ignore>
        <ignore>
          <regexp>@^\s*no@</regexp>
        </ignore>
      </lines>
    </parser>
