  <command name="IMPORT">
    <operation><![CDATA[#cat /config/cisco_ios/sample-config.ios.adress_family.txt]]></operation>
    <parser>
      <section>
        <regexp>@^\s*address-family\s+(?&lt;bgp_address_family&gt;ipv4)$@</regexp>
        <xpath></xpath>
      </section>
      <lines>
        <line>
          <array/>
          <regexp>@^\s+(?&lt;bgp_vrf_default_route&gt;default-information.*)$@</regexp>
          <xpath/>
        </line>
        <line>
          <array name="neighbor">
            <lines/>
            <regexp>@^\s+neighbor\s+(?&lt;bgp_neighbor&gt;\S+)\s+(?&lt;bgp_ipv4_private_as&gt;remove-private-as)@</regexp>
            <regexp>@^\s+neighbor\s+(?&lt;bgp_neighbor&gt;\S+)\s+route-map\s+(?&lt;bgp_ipv4_route_policy_in&gt;\S+)\s+in@</regexp>
            <regexp>@^\s+neighbor\s+(?&lt;bgp_neighbor&gt;\S+)\s+route-map\s+(?&lt;bgp_ipv4_route_policy_out&gt;\S+)\s+out@</regexp>
            <regexp>@^\s+neighbor\s+(?&lt;bgp_neighbor&gt;\S+)\s+maximum-prefix\s+(?&lt;bgp_vrf_maximum_prefix&gt;.+)@</regexp>
            <regexp>@^\s+neighbor\s+(?&lt;bgp_neighbor&gt;\S+)\s+(?&lt;bgp_ipv4_default_route&gt;default-originate)@</regexp>
            <regexp>@^\s+neighbor\s+(?&lt;bgp_neighbor&gt;\S+)\s+remote-as\s+(?&lt;bgp_vrf_neighbor_remote_as&gt;\d+)@</regexp>
            <regexp>@^\s+neighbor\s+(?&lt;bgp_neighbor&gt;\S+)\s+(?&lt;bgp_vrf_as_override&gt;as-override)@</regexp>
            <regexp>@^\s+neighbor\s+(?&lt;bgp_neighbor&gt;\S+)\s+prefix-list\s+(?&lt;bgp_vrf_prefix_list_out&gt;\S+)\s+out@</regexp>
            <regexp>@^\s+neighbor\s+(?&lt;bgp_neighbor&gt;\S+)\s+prefix-list\s+(?&lt;bgp_vrf_route_map_in&gt;\S+)\s+in@</regexp>
            <regexp>@^\s+neighbor\s+(?&lt;bgp_neighbor&gt;\S+)\s+version\s+(?&lt;bgp_vrf_version&gt;\d+)@</regexp>
            <regexp>@^\s+neighbor\s+(?&lt;bgp_neighbor&gt;\S+)\s+(?&lt;bgp_vrf_send_community&gt;send-community.*)@</regexp>
            <regexp>@^\s+neighbor\s+(?&lt;bgp_neighbor&gt;\S+)\s+advertisement-interval\s+(?&lt;bgp_vrf_advertisement_interval&gt;\d+)@</regexp>
            <regexp>@^\s+neighbor\s+(?&lt;bgp_neighbor&gt;\S+)\s+allowas-in\s+(?&lt;bgp_vrf_allow_as&gt;\d+)@</regexp>
            <regexp>@^\s+neighbor\s+(?&lt;bgp_neighbor&gt;\S+)\s+route-map\s+(?&lt;bgp_route_map_in&gt;\S+)\s+in$@</regexp>
            <regexp>@^\s+neighbor\s+(?&lt;bgp_neighbor&gt;\S+)\s+route-map\s+(?&lt;bgp_route_map_out&gt;\S+)\s+out$@</regexp>
            <regexp>@^\s+neighbor\s+(?&lt;bgp_neighbor&gt;\S+)\s+passwort\s+\d\s+(?&lt;bgp_neighbor_passwordt&gt;\S+)$@</regexp>
          </array>
          <regexp/>
          <xpath/>
        </line>
        <line>
          <array/>
          <regexp>@^\s*redistribute\s+(?&lt;bgp_vrf_redistribute&gt;static|connected)$@</regexp>
          <xpath/>
        </line>
        <line>
          <array/>
          <regexp>@^\s*redistribute\s+(?&lt;bgp_vrf_redistrib_policy&gt;static)\s+route-map\s+(?&lt;bgp_vrf_redistrib_static_policy&gt;\S+)$@</regexp>
          <xpath/>
        </line>
        <ignore>
          <regexp>@^\s*exit@</regexp>
        </ignore>
        <ignore>
          <regexp>@^\s*no@</regexp>
        </ignore>
        <ignore>
          <regexp>@^\s*default-information@</regexp>
        </ignore>
        <ignore>
          <regexp>@.*!@</regexp>
        </ignore>
        <ignore>
          <regexp>@.*send-community@</regexp>
        </ignore>
        <ignore>
          <regexp>@.*next-hop-self@</regexp>
        </ignore>
        <ignore>
          <regexp>@.*activate@</regexp>
        </ignore>
        <ignore>
          <regexp>@^\s*maximum-paths@</regexp>
        </ignore>
        <ignore>
          <regexp>@^\s*redistribute@</regexp>
        </ignore>
        <ignore>
          <regexp>@^\s*neighbor\s+to_route_reflector@</regexp>
        </ignore>
        <ignore>
          <regexp>@^\s*address-family ipv4 vrf@</regexp>
        </ignore>
      </lines>
    </parser>
