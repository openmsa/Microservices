  <example>
    <content>router bgp 27699
 nsr
 bgp router-id 177.61.178.250
 bgp graceful-restart graceful-reset
 bgp graceful-restart
 address-family ipv4 unicast
  bgp attribute-download
  nexthop trigger-delay critical 100
  nexthop trigger-delay non-critical 2000
  redistribute connected route-policy RP_TO_IPv4_LU
  allocate-label route-policy RP_ALLOW-32
 !</content>
