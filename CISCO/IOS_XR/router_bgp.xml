  <command name="IMPORT">
    <operation><![CDATA[#cat /config/cisco_ios_xr/sample-config.ios-xr.V4815.conf]]></operation>
    <post_template>{assign_object_variable var=&quot;router_bgp&quot; value=&quot;{$params.object_id}&quot;}</post_template>
    <parser>
      <section>
        <regexp>@^\s*router\s+bgp\s+(?&lt;object_id&gt;\d+)$@</regexp>
        <xpath></xpath>
      </section>
      <lines>
        <line>
          <array/>
          <regexp>@^\s*rd\s+(?&lt;rd&gt;[0-9]+:[0-9]+)@</regexp>
          <xpath/>
        </line>
        <line>
          <array/>
          <regexp>@^\s*vrf\s+(?&lt;bgp_vrf_name&gt;\S+)@</regexp>
          <xpath/>
        </line>
        <line>
          <array name="address_family">
            <lines>
              <line>
                <array/>
                <regexp>@^\s*redistribute\s+(?&lt;bgp_vrf_redistribute&gt;static|connected)$@</regexp>
                <xpath/>
              </line>
              <line>
                <array/>
                <regexp>@^\s*redistribute\s+(?&lt;bgp_vrf_redistrib_policy&gt;static)\s+route-policy\s+(?&lt;bgp_vrf_redistrib_static_policy&gt;\S+)$@</regexp>
                <xpath/>
              </line>
            </lines>
            <lines/>
            <regexp>@^\s*address-family\s+(?&lt;bgp_address_family&gt;ipv4)\s+unicast$@</regexp>
          </array>
          <regexp/>
          <xpath/>
        </line>
        <ignore>
          <regexp>@^\s*!@</regexp>
        </ignore>
      </lines>
    </parser>
  </command>
  <command name="CREATE">
    <operation><![CDATA[{if isset($params.neighbors) && (count($params.neighbors)>0)}
!
router bgp {$params.router_bgp}
{foreach $params.neighbors as $neighbor}
 {if isset($neighbor.bgp_remote_as)} neighbor {$neighbor.bgp_neighbor} remote-as {$neighbor.bgp_remote_as}	
 {else if isset($neighbor.bgp_description)} neighbor {$neighbor.bgp_neighbor} description {$neighbor.bgp_description}
 {else if isset($neighbor.bgp_ebgp_multihop)} neighbor {$neighbor.bgp_neighbor} ebgp-multihop {$neighbor.bgp_ebgp_multihop}
 {else if isset($neighbor.bgp_source_interface)} neighbor {$neighbor.bgp_neighbor} update-source {$neighbor.bgp_source_interface}
 {else if isset($neighbor.bgp_password)} neighbor {$neighbor.bgp_neighbor} password {$neighbor.bgp_password}
{/if}
{/foreach}
!
root
{/if}]]></operation>
  </command>
