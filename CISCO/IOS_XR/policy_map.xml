  <command name="IMPORT">
    <operation><![CDATA[#cat /config/cisco_ios_xr/sample-config.ios-xr.policy_map.txt]]></operation>
    <parser>
      <section>
        <regexp>@\s*policy-map\s+(?&lt;object_id&gt;\S+).*$@</regexp>
        <xpath></xpath>
      </section>
      <lines>
        <line>
          <array name="pmap_class">
            <lines>
              <line>
                <array/>
                <regexp>@^\s*queue limit\s+((?&lt;queue_limit&gt;\d+)( packets)?)$@</regexp>
                <xpath/>
              </line>
              <line>
                <array/>
                <regexp>@^\s*priority level\s*(?&lt;priority&gt;\d+)$@</regexp>
                <xpath/>
              </line>
              <line>
                <array/>
                <regexp>@^\s+service policy\s+(?&lt;service_policy&gt;.*)$@</regexp>
                <xpath/>
              </line>
              <line>
                <array name="random">
                  <regexp>@^\s*random-detect\s*dscp\s*(?&lt;rd_service_codepoint&gt;\d+)\s*(?&lt;rd_min_threshold&gt;\d+)\s*(?&lt;rd_max_threshold&gt;\d+).*$@</regexp>
                </array>
                <regexp/>
                <xpath/>
              </line>
              <line>
                <array/>
                <regexp>@^\s+shape\s+(?&lt;shape&gt;.*)@</regexp>
                <xpath/>
              </line>
              <line>
                <array/>
                <regexp>@^\s+conform-action\s+(?&lt;conform_action&gt;.*)@</regexp>
                <xpath/>
              </line>
              <line>
                <array/>
                <regexp>@^\s+exceed-action\s+(?&lt;exceed_action&gt;.*)@</regexp>
                <xpath/>
              </line>
              <line>
                <array/>
                <regexp>@^\s+(?&lt;fair_queue&gt;(fair-queue))$@</regexp>
                <xpath/>
              </line>
              <line>
                <array name="set">
                  <regexp>@^\s+set\s+(?&lt;set_type&gt;\S+)\s+(?&lt;set_value&gt;.*)@</regexp>
                </array>
                <regexp/>
                <xpath/>
              </line>
              <line>
                <array/>
                <regexp>@^\s*bandwidth\s+((?&lt;bandwidth&gt;\d+)( kbps)?)\s*$@</regexp>
                <xpath/>
              </line>
              <line>
                <array/>
                <regexp>@^\s*police(( rate)?\s*(?&lt;police_cir_burst&gt;\d+)( bps)?)?(( burst)?\s*(?&lt;police_bc_burst&gt;\d+)( bytes)?)?(( peak burst)?\s*(?&lt;police_be_burst&gt;\d+)( bytes)?)?@</regexp>
                <xpath/>
              </line>
              <line>
                <array/>
                <regexp>@^\s*police ((rate\s*(?&lt;police_rate&gt;\d+))?|(rate percent\s*(?&lt;police_rate_percent&gt;\d+))?|(cir percent\s*(?&lt;police_cir_percent&gt;\d+))?)\s*(conform-action (?&lt;conform_action&gt;((?!exceed-action).)*))?\s*(exceed-action (?&lt;exceed_action&gt;((?!violate-action).)*))?\s*(violate-action (?&lt;violate_action&gt;.*))?$@</regexp>
                <xpath/>
              </line>
              <line>
                <array/>
                <regexp>@^\s*violate-action (?&lt;violate_action&gt;.*)$@</regexp>
                <xpath/>
              </line>
            </lines>
            <lines/>
            <regexp>@^\s+class\s+(?&lt;class_name&gt;\S+)@</regexp>
          </array>
          <regexp/>
          <xpath/>
        </line>
        <ignore>
          <regexp>@^\s*!.*$@</regexp>
        </ignore>
        <ignore>
          <regexp>@^\s*end policy-map@</regexp>
        </ignore>
        <ignore>
          <regexp>@^\s*$@</regexp>
        </ignore>
      </lines>
    </parser>
  </command>
  <command name="CREATE">
    <operation><![CDATA[{if isset($params.pmap_class)}
!
policy-map  {$params.object_id}
{foreach $params.pmap_class as $pmap_class}
 class {$pmap_class.class_name} 
  {if isset($pmap_class.queue_limit) && $pmap_class.queue_limit}
  queue-limit {$pmap_class.queue_limit} 
  {/if}
  {if isset($pmap_class.priority) && $pmap_class.priority}
  {$pmap_class.priority} 
  {/if} 
  {if isset($pmap_class.police_cir_burst) && $pmap_class.police_cir_burst}police rate {$pmap_class.police_cir_burst} bps {if isset($pmap_class.police_bc_burst) && $pmap_class.police_bc_burst}burst {$pmap_class.police_bc_burst} {/if}{if isset($pmap_class.police_be_burst) && $pmap_class.police_be_burst} peak-burst {$pmap_class.police_be_burst}{/if}
  {/if}
  {if isset($pmap_class.police_cir_percent) && $pmap_class.police_cir_percent} police rate percent {$pmap_class.police_cir_percent}{if isset($pmap_class.police_bc_burst) && $pmap_class.police_bc_burst} burst {$pmap_class.police_bc_burst} {/if}{if isset($pmap_class.police_be_burst) && $pmap_class.police_be_burst} peak-burst {$pmap_class.police_be_burst}{/if}
  {/if}
  {strip}
  {if isset($pmap_class.police_rate) && $pmap_class.police_rate} police rate {$pmap_class.police_rate} bps 
  {if isset($pmap_class.police_bc_burst) && $pmap_class.police_bc_burst}burst {$pmap_class.police_bc_burst} {/if}
  {if isset($pmap_class.police_be_burst) && $pmap_class.police_be_burst} peak-burst {$pmap_class.police_be_burst}{/if}
  {/if}
  {/strip}
