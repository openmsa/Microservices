  <command name="READ">
    <operation><![CDATA[show hw-module subslot {$params.subslot_num} transceiver {$params.interface_num}  status ]]></operation>
    <post_template>{assign_object_variable 
	var=&quot;object_id&quot;
	value=&quot;{$params.subslot_num}/{$params.transceiver}&quot;}</post_template>
    <parser>
      <section>
        <regexp>@^\s*show hw-module subslot (?&lt;subslot_num&gt;\d+\/\d+) transceiver (?&lt;transceiver&gt;\d+)  status@</regexp>
      </section>
      <lines>
        <line>
