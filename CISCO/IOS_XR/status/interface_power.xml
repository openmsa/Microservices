  <command name="IMPORT">
    <operation><![CDATA[{if isset($params.object_id)}show controllers {$params.object_id} phy {/if}]]></operation>
    <post_template>{assign_object_variable 
	var=&quot;object_id&quot;
	value=&quot;{$params.subslot_num}_transceiver{$params.transceiver}&quot;}</post_template>
    <parser>
      <section>
        <regexp>@^\s*show controllers (?&lt;object_id&gt;.*) phy@</regexp>
      </section>
      <lines>
        <line>
          <array/>
          <regexp>@^\s*Tx power: (?&lt;interface_power_tx&gt;.*dBm )@</regexp>
          <xpath/>
        </line>
        <line>
          <array/>
          <regexp>@^\s*Tx power: (?&lt;interface_power_rx&gt;.*dBm )@</regexp>
          <xpath/>
        </line>
        <ignore>
          <regexp>@The Transceiver@</regexp>
        </ignore>
        <ignore>
          <regexp>@Module@</regexp>
        </ignore>
        <ignore>
          <regexp>@Transceiver Tx supply voltage@</regexp>
        </ignore>
        <ignore>
          <regexp>@D:@</regexp>
        </ignore>
        <ignore>
