  <command name="CREATE">
    <operation><![CDATA[{if isset($params.statements)}
!
route-policy {$params.object_id}
{foreach $params.statements|@sortby_typed:"priority:int" as $statements}
  {$continue=false}
  {$continue_is_set=false}
  {* first iteration on blocks to look for potential "continue" *}
  {if isset($statements.block)}
    {foreach $statements.block as $block}
      {if isset($block.continue) && !$statements@last && !$continue_is_set}
        {$continue=true}
        {break}
      {/if}
    {/foreach}
  {/if}

  {if $statements.action == "deny" && !$statements@last}
	{$action="deny"}
    {*continue*}
  {else}
  	{$action="permit"}
  {/if}
  {$keys = array_keys($statements)}
  {$clean_keys=[]}
  {$idx=0}
  {* clean up the list of keys *}
  {foreach $keys as $key}
    {if $key == "block" || $key == "_order" || $key == "priority" || $key == "action"}
      {continue}
    {/if}
    {$clean_keys[$idx]=$key}
    {$idx=$idx+1}
  {/foreach}
  {$first=true}
  {$last=false}
  {if isset($clean_keys) && ($clean_keys|@count > 0)}
    {strip}
  {foreach $clean_keys as $key}
    {if $statements@first && $first} if {else if ($statements@total>1 && !$statements@first && $first)} elseif {/if}   
    {if $continue==true && !$continue_is_set}{$continue_is_set=true} not {/if}     
    {if $key == "as_path"}{$first_as_path=true}{if $first==false} and {else}{$first=false}{/if}{if $action=="deny"} not {/if}({foreach $statements.as_path as $as_path}{if $first_as_path==false} and {else}{$first_as_path=false}{/if}(as-path in {$as_path.val}){/foreach}){* as-path *}     
    {else if $key == "communities"}{$first_com=true}{if $first==false} and {else}{$first=false}{/if}{if $action=="deny"} not {/if}({foreach $statements.communities as $community}{if $first_com==false} and {else}{$first_com=false}{/if}(community matches-any {$community.val}){/foreach}){* community *}
