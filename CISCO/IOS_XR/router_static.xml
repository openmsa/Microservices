  <command name="IMPORT">
    <operation><![CDATA[#cat /config/cisco_ios_xr/router_static.conf]]></operation>
    <post_template>{assign_object_variable var=&quot;object_id&quot; value=&quot;1&quot;}</post_template>
    <parser>
      <section>
        <regexp>@^\s*router static@</regexp>
        <xpath></xpath>
      </section>
      <lines>
        <line>
          <array name="vrf_routes">
            <lines>
              <line>
                <array name="routes">
                  <lines/>
                  <lines/>
                  <regexp>@^\s*(?&lt;ipv4_address&gt;([0-9]{1,3}\.){3}[0-9]{1,3})\/(?&lt;ipv4_mask_length&gt;\d+)(\s(?&lt;interface&gt;\S+))?(\s(?&lt;gateway&gt;([0-9]{1,3}\.){3}[0-9]{1,3}))?(\s(?&lt;metric&gt;\d+))?(\stag\s(?&lt;tag&gt;\d+))?$@</regexp>
                </array>
                <regexp/>
                <xpath/>
              </line>
              <line>
                <array name="routes6">
                  <lines/>
                  <lines/>
                  <regexp>@^\s*(?&lt;ipv6_address&gt;([A-Fa-f0-9:]*:+)+[A-Fa-f0-9]*)\/(?&lt;ipv6_mask_length&gt;\d+)(\s(?&lt;interface&gt;\S+))?(\s(?&lt;gateway&gt;([A-Fa-f0-9:]*:+)+[A-Fa-f0-9]*))?(\s(?&lt;metric&gt;\d+))?(\stag\s(?&lt;tag&gt;\d+))?$@</regexp>
                </array>
                <regexp/>
                <xpath/>
              </line>
            </lines>
            <lines/>
            <regexp>@^\s*vrf\s+(?&lt;vrf_name&gt;\S+)@</regexp>
          </array>
          <regexp/>
          <xpath/>
        </line>
        <line>
          <array name="routes">
            <lines/>
            <lines/>
            <regexp>@^\s*(?&lt;ipv4_address&gt;([0-9]{1,3}\.){3}[0-9]{1,3})\/(?&lt;ipv4_mask_length&gt;\d+)(\s(?&lt;interface&gt;\S+))?(\s(?&lt;gateway&gt;([0-9]{1,3}\.){3}[0-9]{1,3}))?(\s(?&lt;metric&gt;\d+))?(\stag\s(?&lt;tag&gt;\d+))?$@</regexp>
          </array>
          <regexp/>
          <xpath/>
        </line>
        <line>
          <array name="routes6">
            <lines/>
            <lines/>
            <regexp>@^\s*(?&lt;ipv6_address&gt;([A-Fa-f0-9:]*:+)+[A-Fa-f0-9]*)\/(?&lt;ipv6_mask_length&gt;\d+)(\s?(?&lt;gateway&gt;([A-Fa-f0-9:]*:+)+[A-Fa-f0-9]*))?(\s(?&lt;metric&gt;\d+))?(\stag\s(?&lt;tag&gt;\d+))?$@</regexp>
          </array>
          <regexp/>
          <xpath/>
        </line>
        <ignore>
          <regexp>@^\s*!@</regexp>
        </ignore>
        <ignore>
          <regexp>@address-family@</regexp>
        </ignore>
      </lines>
    </parser>
