  <command name="CREATE">
    <operation><![CDATA[{if (isset($params.routes) && count($params.routes) > 0)
  || (isset($params.routes6) && count($params.routes6) > 0)
  || (isset($params.vrf_routes) && count($params.vrf_routes) > 0)
  || (isset($params.vrf_routes6) && count($params.vrf_routes6) > 0) }
!
router static
{if isset ($params.routes)}
{foreach $params.routes as $route}
  {if $route@first}
 address-family ipv4 unicast
  {/if}

  {$route.ipv4_address}/{$route.ipv4_mask_length} {if isset($route.interface)}{$route.interface}{/if} {$route.gateway} {if isset($route.metric)}{$route.metric}{/if}{if isset($route.tag)} tag {$route.tag}
{/if}
{/foreach}

!
{/if}
{if isset ($params.routes6)}
{foreach $params.routes6 as $route}
  {if $route@first}
 address-family ipv6 unicast
  {/if}
  {$route.ipv6_address}/{$route.ipv6_mask_length} {if isset($route.interface)}{$route.interface}{/if} {$route.gateway} {if isset($route.metric)}{$route.metric}{/if}{if isset($route.tag)} tag {$route.tag}`
{/if}
{/foreach}

!
{/if}

{if isset ($params.vrf_routes)}
{foreach $params.vrf_routes as $vrf}
{if isset($vrf.vrf_name) && isset($vrf.routes)}
vrf {$vrf.vrf_name} address-family ipv4 unicast
{/if}
{foreach $vrf.routes as $route}
  
  {$route.ipv4_address}/{$route.ipv4_mask_length} {if isset($route.interface)}{$route.interface}{/if} {$route.gateway} {if isset($route.metric)}{$route.metric}{/if}{if isset($route.tag)} tag {$route.tag}
{/if}
{/foreach}
{/foreach}

!
{/if}

{if isset ($params.vrf_routes6)}
{foreach $params.vrf_routes6 as $vrf}
{if isset($vrf.vrf_name) && isset($vrf.routes)}
vrf {$vrf.vrf_name}  address-family ipv6 unicast
{/if}
{foreach $vrf.routes as $route}

  {$route.ipv6_address}/{$route.ipv6_mask_length} {if isset($route.interface)}{$route.interface}{/if} {$route.gateway} {if isset($route.metric)}{$route.metric}{/if}{if isset($route.tag)} tag {$route.tag}
{/if}
{/foreach}
{/foreach}

!
{/if}
root
{/if}
]]></operation>
