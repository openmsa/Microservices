  <command name="CREATE">
    <operation><![CDATA[
{assign 'mergedTCPandUDPPorts' $FTDv_tcpports|array_merge:$FTDv_udpports}
{assign 'mergedPorts' $mergedTCPandUDPPorts|array_merge:$FTDv_icmpv4ports}
{function getPortType}{foreach from=$portsArray item=port}{if $port.object_id == $searchedPortId}{$port.type}{break}{/if}{/foreach}
{/function}
{ 
        "destinationZones": [{$coma=""}
              {foreach from=$params.destinationZones item=destZone}
                 {$coma} {ldelim}
                  "id":"{$destZone.destinationZoneName}",
                  "type":"securityzone"
                 {rdelim}{$coma=","}
