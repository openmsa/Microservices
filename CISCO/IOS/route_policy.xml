  <command name="IMPORT">
    <operation><![CDATA[show running-config | section route-map]]></operation>
    <post_template>{assign var=filtered_route_map value=[]}
{assign var=route_map_idx value=0}
{$name=&quot;&quot;}
{foreach $route_map as $rm}
	{if $params.object_id == $rm.name}
		{$filtered_route_map[$route_map_idx]=$rm}
		{$name=$rm.name}
	{/if}
	{$route_map_idx=$route_map_idx+1}
{/foreach}
{$statement_idx=0}
{if isset($filtered_route_map)}
	{capture}
	{ksort($filtered_route_map)}
	{/capture}
	{foreach $filtered_route_map|@sortby_typed:&quot;priority:int&quot; as $rm}
{*==&gt; {$rm._order} - {$rm.name} {$rm.action} {$rm.priority}*}
		{assign_object_variable
			var=&quot;statements.{$statement_idx}._order&quot;
			value=&quot;{$rm._order}&quot;}
		{assign_object_variable
			var=&quot;statements.{$statement_idx}.action&quot;
			value=&quot;{$rm.action}&quot;}
		{assign_object_variable
			var=&quot;statements.{$statement_idx}.priority&quot;
			value=&quot;{$rm.priority}&quot;}
		{if isset($rm.sorted_matches)}
			{$as_path_idx=0}
			{$comm_idx=0}					
			{$extcomm_idx=0}					
			{$extcommset_idx=0}					
			{$dest_idx=0}
			{$tag_idx=0}
			{$ip_next_hop_idx=0}
			{$interface_idx=0}
			{foreach $rm.sorted_matches	as $match}
				{if isset($match.as_path)}
					{assign_object_variable
						var=&quot;statements.{$statement_idx}.as_path.{$as_path_idx}.val&quot;
						value=&quot;as_path_set_{$match.as_path}&quot;}
					{$as_path_idx=$as_path_idx+1}
				{/if}
				{if isset($match.community)}
					{assign_object_variable
						var=&quot;statements.{$statement_idx}.communities.{$comm_idx}.val&quot;
						value=&quot;{$match.community}&quot;}
					{$comm_idx=$comm_idx+1}
				{/if}
				{if isset($match.community_set)}
					{foreach $match.community_set as $comm_set}
					    {assign var=comm_list value=&quot; &quot;|explode:$comm_set}
            			{foreach $comm_list as $comm}
							{assign_object_variable
								var=&quot;statements.{$statement_idx}.communities.{$comm_idx}.val&quot;
								value=&quot;comm_set_{$comm}&quot;}
							{$comm_idx=$comm_idx+1}
						{/foreach}
					{/foreach}
				{/if}
				{if isset($match.extcommunity)}
					{assign_object_variable
						var=&quot;statements.{$statement_idx}.extcommunities.{$extcomm_idx}.val&quot;
						value=&quot;{$match.extcommunity}&quot;}
					{$extcomm_idx=$extcomm_idx+1}
				{/if}
				{if isset($match.extcommunity_set)}
					{foreach $match.extcommunity_set as $extcomm_set}
					    {assign var=extcomm_list value=&quot; &quot;|explode:$extcomm_set}
            			{foreach $extcomm_list as $extcomm}
							{assign_object_variable
								var=&quot;statements.{$statement_idx}.extcommunities.{$extcomm_idx}.val&quot;
								value=&quot;{$extcomm}&quot;}
							{$extcomm_idx=$extcomm_idx+1}
						{/foreach}
					{/foreach}
				{/if}
				{if isset($match.ip_access_list)}
					{assign var=ip_access_list value=&quot; &quot;|explode:$match.ip_access_list}
					{$transformed_acl=&quot;&quot;}
					{foreach $ip_access_list as $ip_acl}
						{$transformed_acl=&quot;pfx_{$ip_acl}&quot;}
						{assign_object_variable
							var=&quot;statements.{$statement_idx}.destination_acl.{$dest_idx}.val&quot;
							value=&quot;{$transformed_acl}&quot;}
						{$dest_idx=$dest_idx+1}
					{/foreach}
				{/if}
				{if isset($match.ipv6_access_list)}
					{assign_object_variable
						var=&quot;statements.{$statement_idx}.destination_acl.{$dest_idx}.val&quot;
						value=&quot;pfx_{$match.ipv6_access_list}&quot;}
					{$dest_idx=$dest_idx+1}
				{/if}
				{if isset($match.ip_prefix_list)}
					{assign_object_variable
						var=&quot;statements.{$statement_idx}.destination_pfx.{$dest_idx}.val&quot;
						value=&quot;pfx_{$match.ip_prefix_list}&quot;}
					{$dest_idx=$dest_idx+1}
				{/if}
				{if isset($match.ipv6_prefix_list)}
					{assign_object_variable
						var=&quot;statements.{$statement_idx}.destination_pfx.{$dest_idx}.val&quot;
						value=&quot;pfx_{$match.ipv6_prefix_list}&quot;}
					{$dest_idx=$dest_idx+1}
				{/if}
				{if isset($match.tag)}
					{assign_object_variable
						var=&quot;statements.{$statement_idx}.tag.{$tag_idx}.val&quot;
						value=&quot;{$match.tag}&quot;}
					{$tag_idx=$tag_idx+1}
				{/if}
				{if isset($match.ip_next_hop)}
					{assign_object_variable
						var=&quot;statements.{$statement_idx}.next_hop.{$ip_next_hop_idx}.val&quot;
						value=&quot;{$match.ip_next_hop}&quot;}
					{$ip_next_hop_idx=$ip_next_hop_idx+1}
				{/if}
				{if isset($match.ipv6_next_hop)}
					{assign_object_variable
						var=&quot;statements.{$statement_idx}.next_hop.{$ip_next_hop_idx}.val&quot;
						value=&quot;{$match.ipv6_next_hop}&quot;}
					{$ip_next_hop_idx=$ip_next_hop_idx+1}
				{/if}
				{if isset($match.interface)}
					{* loop through the loopback_interface microservice to get the ip *}
					{if isset($interface_loopback)}
						{foreach $interface_loopback as $loopback}
						{if ($loopback.object_id == $match.interface)}
							{if isset($loopback.ipv4_address) &amp;&amp; isset($loopback.ipv4_prefix)}
								{assign_object_variable
									var=&quot;statements.{$statement_idx}.destination_itf.{$dest_idx}.val&quot;
									value=&quot;({$loopback.ipv4_address}/{$loopback.ipv4_prefix})&quot;}
								{$dest_idx=$dest_idx+1}
								{break}
							{else if isset($loopback.ipv6_address) &amp;&amp; isset($loopback.ipv6_prefix)}
								{assign_object_variable
									var=&quot;statements.{$statement_idx}.destination_itf.{$dest_idx}.val&quot;
									value=&quot;({$loopback.ipv6_address}/{$loopback.ipv6_prefix})&quot;}
								{$dest_idx=$dest_idx+1}
								{break}
							{/if}

						{/if}
						{/foreach}
					{/if}
				{/if}
			{/foreach}
		{/if}
		{$block_idx=0}
		{if isset($rm.sets)}
			{foreach $rm.sets as $set}
				{if	isset($set.operator)}
					{assign_object_variable
						var=&quot;statements.{$statement_idx}.block.{$block_idx}.operator&quot;
						value=&quot;{$set.operator}&quot;}
					{$block_idx=$block_idx+1}
				{/if}
				{if	isset($set.continue)}
					{assign_object_variable
					var=&quot;statements.{$statement_idx}.block.{$block_idx}.continue&quot;
					value=&quot;{$set.continue}&quot;}
					{$block_idx=$block_idx+1}
				{/if}
				{if isset($set.weight)}
					{assign_object_variable
						var=&quot;statements.{$statement_idx}.block.{$block_idx}.weight&quot;
						value=&quot;{$set.weight}&quot;}
					{$block_idx=$block_idx+1}
				{/if}
				{if isset($set.metric)}
					{assign_object_variable
						var=&quot;statements.{$statement_idx}.block.{$block_idx}.metric&quot;
						value=&quot;{$set.metric}&quot;}
					{$block_idx=$block_idx+1}
				{/if}
				{if isset($set.ip_next_hop)}
					{assign_object_variable
						var=&quot;statements.{$statement_idx}.block.{$block_idx}.next_hop&quot;
						value=&quot;{$set.ip_next_hop}&quot;}
					{$block_idx=$block_idx+1}
				{/if}
				{if isset($set.ipv6_next_hop)}
					{assign_object_variable
						var=&quot;statements.{$statement_idx}.block.{$block_idx}.next_hop&quot;
						value=&quot;{$set.ipv6_next_hop}&quot;}
					{$block_idx=$block_idx+1}
				{/if}
				{if	isset($set.local_preference)}
					{assign_object_variable
						var=&quot;statements.{$statement_idx}.block.{$block_idx}.local_preference&quot;
						value=&quot;{$set.local_preference}&quot;}
					{$block_idx=$block_idx+1}
				{/if}
				{if isset($set.extcommunity_set)}
					{foreach $set.extcommunity_set as $extcomm_set}
						{assign_object_variable
							var=&quot;statements.{$statement_idx}.block.{$block_idx}.extcommunity.{$extcomm_set@index}.extcommunity_set&quot;
							value=&quot;{$extcomm_set.extcommunity}&quot;}
					{/foreach}
					{if isset($set.extcommunity_type)}
						{assign_object_variable
							var=&quot;statements.{$statement_idx}.block.{$block_idx}.extcommunity_type&quot;
							value=&quot;{$set.extcommunity_type}&quot;}
					{/if}
					{$block_idx=$block_idx+1}
				{/if}
				{if isset($set.community_set)}
					{foreach $set.community_set as $comm_set}
						{assign_object_variable
							var=&quot;statements.{$statement_idx}.block.{$block_idx}.community.{$comm_set@index}.community_set&quot;
							value=&quot;{$comm_set.community}&quot;}
					{/foreach}
					{$block_idx=$block_idx+1}
				{/if}
			{/foreach}
		{/if}
		{$statement_idx=$statement_idx+1}
	{/foreach}
{/if}
{assign_object_variable
	var=&quot;object_id&quot;
	value=&quot;{$name|replace:&apos;+&apos;:&apos;_&apos;}&quot;}
</post_template>
