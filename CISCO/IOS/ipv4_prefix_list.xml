  <command name="IMPORT">
    <operation>show running-config</operation>
    <post_template>{$idx=0}
{* foreach $ip_prefix_list|@sortby_as_int:&quot;sequence&quot; as $ipl *}
{foreach $ip_prefix_list|@sortby:&quot;sequence&quot; as $ipl}
	{if isset($ipl.sequence)}
		{if ($ipl.name == $params.object_id)}
			{if isset($ipl.sequence)}
				{assign_object_variable
					var=&quot;ip_prefix_list.{$idx}.sequence&quot;
					value=&quot;{$ipl.sequence}&quot;}
			{/if}
			{if isset($ipl.action)}
				{assign_object_variable
					var=&quot;ip_prefix_list.{$idx}.action&quot;
					value=&quot;{$ipl.action}&quot;}
			{/if}
