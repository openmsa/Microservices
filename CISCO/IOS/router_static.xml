  <example/>
  <command name="IMPORT">
    <operation><![CDATA[show running-config]]></operation>
    <post_template>{$route_idx=0}
{$route6_idx=0}
{$_vrf_routes=[]}

{* loop through the ip route and process the VRF routes to build a local array $_vrf_routes with the VRF routes *}
{foreach $ip_route as $route}
	{$vrf_idx=0}
	{if $route.vrf_name &amp;&amp; isset($route.vrf_name)}
		{* check if the VRF was already found *}
		{$found=false}
		{if isset($route.ipv4_mask)}
			{$mask=$route.ipv4_mask}
			{capture}
			{get_mask_len mask=$mask var=&quot;mask_length&quot;}
			{/capture}
		{/if}
		{foreach $_vrf_routes as $_vrf_route}
			{if $_vrf_route.vrf_name == $route.vrf_name}
				{$found=true}
				{break}
			{/if}
			{$vrf_idx=$vrf_idx+1}
		{/foreach}
		{if !$found}
			{* if the VRF is a newly found one, initialize the VRF $routes array by adding data as the first element (index 0) *}
			{$_vrf_routes.{$vrf_idx}.routes=[]}
			{$_vrf_routes.{$vrf_idx}.routes6=[]}
			{if $route.type == &quot;ip&quot;}
				{$_vrf_routes.{$vrf_idx}.vrf_name=$route.vrf_name}
				{$_vrf_routes.{$vrf_idx}.routes.0.ipv4_address=$route.ipv4_address}
				{$_vrf_routes.{$vrf_idx}.routes.0.ipv4_mask_length=$mask_length}
				{if isset($route.interface)}
					{$_vrf_routes.{$vrf_idx}.routes.0.interface=$route.interface}
				{/if}
				{if isset($route.gateway)}
					{$_vrf_routes.{$vrf_idx}.routes.0.gateway=$route.gateway}
				{/if}
				{if isset($route.metric)}
					{$_vrf_routes.{$vrf_idx}.routes.0.metric=$route.metric}
				{/if}
				{if isset($route.tag)}
					{$_vrf_routes.{$vrf_idx}.routes.0.tag=$route.tag}
				{/if}
			{else if $route.type == &quot;ipv6&quot;}
				{$_vrf_routes.{$vrf_idx}.vrf_name=$route.vrf_name}
				{$_vrf_routes.{$vrf_idx}.routes6.0.ipv6_address=$route.ipv6_address}
				{$_vrf_routes.{$vrf_idx}.routes6.0.ipv6_mask_length=$route.ipv6_mask_length}
				{if isset($route.interface)}
					{$_vrf_routes.{$vrf_idx}.routes6.0.interface=$route.interface}
				{/if}
				{if isset($route.gateway)}
					{$_vrf_routes.{$vrf_idx}.routes6.0.gateway=$route.gateway}
				{/if}
				{if isset($route.metric)}
					{$_vrf_routes.{$vrf_idx}.routes6.0.metric=$route.metric}
				{/if}
				{if isset($route.tag)}
					{$_vrf_routes.{$vrf_idx}.routes6.0.tag=$route.tag}
				{/if}
			{/if}
		{else}
			{* if it&apos;s a VRF that was already processed, add the new route to the $routes array *}
			{if $route.type == &quot;ip&quot;}
				{* simple foreach to get the size of $_vrf_route array *}
				{foreach $_vrf_routes.{$vrf_idx}.routes as $_route}
					{$route_idx=$_route@total}
					{break}
				{/foreach}
				{$_vrf_routes.{$vrf_idx}.routes.{$route_idx}.ipv4_address=$route.ipv4_address}
				{$_vrf_routes.{$vrf_idx}.routes.{$route_idx}.ipv4_mask_length=$mask_length}
				{if isset($route.interface)}
					{$_vrf_routes.{$vrf_idx}.routes.{$route_idx}.interface=$route.interface}
				{/if}
				{if isset($route.gateway)}
					{$_vrf_routes.{$vrf_idx}.routes.{$route_idx}.gateway=$route.gateway}
				{/if}
				{if isset($route.metric)}
					{$_vrf_routes.{$vrf_idx}.routes.{$route_idx}.metric=$route.metric}
				{/if}
				{if isset($route.tag)}
					{$_vrf_routes.{$vrf_idx}.routes.{$route_idx}.tag=$route.tag}
				{/if}
			{else if $route.type == &quot;ipv6&quot;}
				{* simple foreach to get the size of $_vrf_route6 array *}
				{foreach $_vrf_routes.{$vrf_idx}.routes6 as $_route6}
					{$route6_idx=$_route6@total}
					{break}
				{/foreach}
				{$_vrf_routes.{$vrf_idx}.routes6.{$route6_idx}.ipv6_address=$route.ipv6_address}
				{$_vrf_routes.{$vrf_idx}.routes6.{$route6_idx}.ipv6_mask_length=$route.ipv6_mask_length}
				{if isset($route.interface)}
					{$_vrf_routes.{$vrf_idx}.routes6.{$route6_idx}.interface=$route.interface}
				{/if}
				{if isset($route.gateway)}
					{$_vrf_routes.{$vrf_idx}.routes6.{$route6_idx}.gateway=$route.gateway}
				{/if}
				{if isset($route.metric)}
					{$_vrf_routes.{$vrf_idx}.routes6.{$route6_idx}.metric=$route.metric}
				{/if}
				{if isset($route.tag)}
					{$_vrf_routes.{$vrf_idx}.routes6.{$route6_idx}.tag=$route.tag}
				{/if}
			{/if}
		{/if}
	{/if}
{/foreach}
{* iterate through the local array $_vrf_routes and assign the values to the microservice variable $vrf_routes *}
{foreach $_vrf_routes as $_vrf_route}
	{assign_object_variable
        var=&quot;vrf_routes.{$_vrf_route@index}.vrf_name&quot;
        value=&quot;{$_vrf_route.vrf_name}&quot;}
    {if isset($_vrf_route.routes)}
		{foreach $_vrf_route.routes as $_route}
			{assign_object_variable var=&quot;vrf_routes.{$_vrf_route@index}.routes.{$_route@index}.ipv4_address&quot; 
				value=&quot;{$_route.ipv4_address}&quot;}
			{assign_object_variable var=&quot;vrf_routes.{$_vrf_route@index}.routes.{$_route@index}.ipv4_mask_length&quot; 
				value=&quot;{$_route.ipv4_mask_length}&quot;}
			{if isset($_route.interface)}
				{assign_object_variable var=&quot;vrf_routes.{$_vrf_route@index}.routes.{$_route@index}.interface&quot; 
					value=&quot;{$_route.interface}&quot;}
			{/if}
			{if isset($_route.gateway)}
				{assign_object_variable var=&quot;vrf_routes.{$_vrf_route@index}.routes.{$_route@index}.gateway&quot; 
					value=&quot;{$_route.gateway}&quot;}
			{/if}
			{if isset($_route.metric)}
				{assign_object_variable var=&quot;vrf_routes.{$_vrf_route@index}.routes.{$_route@index}.metric&quot; 
					value=&quot;{$_route.metric}&quot;}
			{/if}
			{if isset($_route.tag)}
				{assign_object_variable var=&quot;vrf_routes.{$_vrf_route@index}.routes.{$_route@index}.tag&quot; 
					value=&quot;{$_route.tag}&quot;}
			{/if}
		{/foreach}
	{/if}
    {if isset($_vrf_route.routes6)}
		{foreach $_vrf_route.routes6 as $_route}
			{assign_object_variable var=&quot;vrf_routes.{$_vrf_route@index}.routes6.{$_route@index}.ipv6_address&quot; 
				value=&quot;{$_route.ipv6_address}&quot;}
			{assign_object_variable var=&quot;vrf_routes.{$_vrf_route@index}.routes6.{$_route@index}.ipv6_mask_length&quot; 
				value=&quot;{$_route.ipv6_mask_length}&quot;}
			{if isset($_route.interface)}
				{assign_object_variable var=&quot;vrf_routes.{$_vrf_route@index}.routes6.{$_route@index}.interface&quot; 
					value=&quot;{$_route.interface}&quot;}
			{/if}
			{if isset($_route.gateway)}
				{assign_object_variable var=&quot;vrf_routes.{$_vrf_route@index}.routes6.{$_route@index}.gateway&quot; 
					value=&quot;{$_route.gateway}&quot;}
			{/if}
			{if isset($_route.metric)}
				{assign_object_variable var=&quot;vrf_routes.{$_vrf_route@index}.routes6.{$_route@index}.metric&quot; 
					value=&quot;{$_route.metric}&quot;}
			{/if}
			{if isset($_route.tag)}
				{assign_object_variable var=&quot;vrf_routes.{$_vrf_route@index}.routes6.{$_route@index}.tag&quot; 
					value=&quot;{$_route.tag}&quot;}
			{/if}
		{/foreach}
	{/if}
{/foreach}

{* process the routes that are not VRF routes *}
{foreach $ip_route as $route}
	{if !$route.vrf_name}
		{if isset($route.type) &amp;&amp; $route.type == &quot;ip&quot;}
			{assign_object_variable
				var=&quot;routes.{$route_idx}.ipv4_address&quot; 
				value=&quot;{$route.ipv4_address}&quot;}
			{if isset($route.ipv4_mask)}
			    {$mask=$route.ipv4_mask}
			    {capture} {* transform the mask to the mask length for compatibility with XR *}
			    {get_mask_len mask=$mask var=&quot;mask_length&quot;}
			    {/capture}		
				{assign_object_variable 
					var=&quot;routes.{$route_idx}.ipv4_mask_length&quot; 
					value=&quot;{$mask_length}&quot;}
			{/if}
			{if isset($route.interface)}
				{assign_object_variable 
					var=&quot;routes.{$route_idx}.interface&quot; 
					value=&quot;{$route.interface}&quot;}
			{/if}
			{if isset($route.gateway)}
				{assign_object_variable 
					var=&quot;routes.{$route_idx}.gateway&quot; 
					value=&quot;{$route.gateway}&quot;}
			{/if}
			{if isset($route.metric)}
				{assign_object_variable 
					var=&quot;routes.{$route_idx}.metric&quot; 
					value=&quot;{$route.metric}&quot;}
			{/if}
			{if	isset($route.tag)}
				{assign_object_variable 
					var=&quot;routes.{$route_idx}.tag&quot; 
					value=&quot;{$route.tag}&quot;}
			{/if}
			{$route_idx=$route_idx+1}
		{else if isset($route.type) &amp;&amp; $route.type == &quot;ipv6&quot;}
			{assign_object_variable
				var=&quot;routes6.{$route6_idx}.ipv6_address&quot; 
				value=&quot;{$route.ipv6_address}&quot;}
			{assign_object_variable 
				var=&quot;routes6.{$route6_idx}.ipv6_mask_length&quot; 
				value=&quot;{$route.ipv6_mask_length}&quot;}
			{if isset($route.interface)}
				{assign_object_variable 
					var=&quot;routes6.{$route6_idx}.interface&quot; 
					value=&quot;{$route.interface}&quot;}
			{/if}
			{if isset($route.gateway)}
				{assign_object_variable 
					var=&quot;routes6.{$route6_idx}.gateway&quot; 
					value=&quot;{$route.gateway}&quot;}
			{/if}
			{if isset($route.metric)}
				{assign_object_variable 
					var=&quot;routes6.{$route6_idx}.metric&quot; 
					value=&quot;{$route.metric}&quot;}
			{/if}
			{if	isset($route.tag)}
				{assign_object_variable 
					var=&quot;routes6.{$route6_idx}.tag&quot; 
					value=&quot;{$route.tag}&quot;}
			{/if}
			{$route6_idx=$route6_idx+1}
		{/if}
	{/if}
{/foreach}</post_template>
