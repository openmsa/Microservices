    <post_template></post_template>
    <parser>
      <section>
        <regexp>@^\s*BGP neighbor is (?&lt;bgp_neighbor&gt;.*) external link$@</regexp>
      </section>
      <lines>
        <line>
          <array/>
          <regexp>@\s*Prefixes Total:\s+(?&lt;bgp_vrf_nghr_prefix_total&gt;.*)$@</regexp>
          <xpath/>
        </line>
        <line>
          <array/>
          <regexp>@\s*Neighbor (?&lt;neighbor&gt;.*):$@</regexp>
          <xpath/>
        </line>
        <line>
          <array/>
          <regexp>@\s*Prefixes Current:\s+(?&lt;bgp_vrf_nghr_prefix_cur&gt;\S+)$@</regexp>
          <xpath/>
        </line>
        <line>
          <array/>
          <regexp>@\s+BGP state =(?&lt;bgp_state&gt;.*)$@</regexp>
          <xpath/>
        </line>
        <ignore>
          <regexp>@^\s*$@</regexp>
        </ignore>
        <ignore>
          <regexp>@^\s*!@</regexp>
        </ignore>
        <ignore>
          <regexp>@^$@</regexp>
        </ignore>
        <ignore>
          <regexp>@BGP version@</regexp>
        </ignore>
