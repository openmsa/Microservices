        </line>
        <line>
          <array/>
          <regexp>@&lt;is_public&gt;(?&lt;is_public&gt;.*)&lt;/is_public&gt;@</regexp>
          <xpath>is_public</xpath>
        </line>
      </lines>
    </parser>
    <xpath>/{$TENANT_ID}/flavors/detail</xpath>
  </command>
  <command name="CREATE">
    <operation><![CDATA[{literal}{{/literal}
"flavor": {literal}{{/literal}
"name": "{$params.name}",
