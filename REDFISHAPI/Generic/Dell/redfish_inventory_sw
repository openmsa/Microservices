<command name="IMPORT">
    <operation>GET</operation>
    <xpath>{foreach from=$redfish_inventory_sw_links item=sw_link}{$sw_link.object_id}##{/foreach}</xpath>
    <parser>
      <section>
        <xpath>//root</xpath>
      </section>
      <lines>
        <line>
          <xpath>//root/ATodataDOTid</xpath>
          <regexp>@&lt;ATodataDOTid&gt;(?&lt;odata_id&gt;\S+?)&lt;/ATodataDOTid&gt;@</regexp>
        </line>
        <line>
          <xpath>//root/Oem</xpath>
          <regexp>@&lt;UpdateState&gt;(?&lt;update_state&gt;.*)&lt;/UpdateState&gt;@</regexp>
        </line>
        <line>
          <xpath>//root/Id</xpath>
          <regexp>@&lt;Id&gt;(?&lt;object_id&gt;\S+?)&lt;/Id&gt;@</regexp>
        </line>
        
        
        <line>
          <xpath>//root/Version</xpath>
          <regexp>@&lt;Version&gt;(?&lt;version&gt;\S+)\s*.*?&lt;/Version&gt;@</regexp>
        </line>
        <line>
          <xpath>//root/Actions/Oem</xpath>
          <regexp>@&lt;HASH_SIGNIntelDOTOemDOTUpdate[^&gt;]+?&gt;&lt;target&gt;(?&lt;target&gt;.+?)&lt;/target&gt;.*?&lt;/HASH_SIGNIntelDOTOemDOTUpdate[^&gt;]+?&gt;@</regexp>
        </line>
      </lines>
    </parser>
    <post_template>{assign var="link_array" value='/'|explode:$params.odata_id}
{assign var="id" value="{$link_array|@end}"}
{if !$params.object_id neq $id}
   {assign_object_variable var="object_id" value=$id}
{/if}
{if !$params.target}
   {assign_object_variable var="target" value="NULL"}
{/if}</post_template>
  </command>
<command name="UPDATE">
    <rest>POST
</rest>
    <xpath>{$redfish_inventory_sw.$object_id.target}
</xpath>
    <operation>FORM_DATA_UPLOADING#{foreach from=$params.file_list key=k item=i}{$params.form_list[$k]}=@{$i}***{/foreach}#
