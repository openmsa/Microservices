</rest>
    <xpath>/api/dcim/devices/
</xpath>
    <operation>{literal}{{/literal}
	"name": "{$params.object_id}"
	, "status": "{$params.status}"
	{foreach $device_types item=type}
		{if $type.object_id eq $params.model}
