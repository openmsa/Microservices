  <command name="IMPORT">
    <operation>GET</operation>
    <parser>
      <section>
        <regexp>@^\s \s*$@</regexp>
        <xpath>//results/row</xpath>
      </section>
      <lines>
        <line>
          <array name="tags">
            <xpath>/row/tags/row</xpath>
            <lines>
              <line>
                <xpath>//row</xpath>
                <regexp>@&lt;row&gt;(?&lt;tag&gt;.+?)&lt;/row&gt;@</regexp>
              </line>
            </lines>
          </array>
        </line>
        <line>
          <array/>
          <regexp>@&lt;name&gt;(?&lt;site&gt;.+?)&lt;/name&gt;@</regexp>
          <xpath>/row/site/name</xpath>
        </line>
        <line>
          <array/>
          <regexp>@&lt;vid&gt;(?&lt;vlan_id&gt;\d{1,4})&lt;/vid&gt;@</regexp>
          <xpath>/row/vlan/vid</xpath>
        </line>
        <line>
          <array/>
          <regexp>@&lt;name&gt;(?&lt;vlan_name&gt;.+?)&lt;/name&gt;@</regexp>
          <xpath>/row/vlan/name</xpath>
        </line>
        <line>
          <array/>
          <regexp>@&lt;name&gt;(?&lt;tenant&gt;.+?)&lt;/name&gt;@</regexp>
          <xpath>/row/tenant/name</xpath>
        </line>
        <line>
          <array/>
          <regexp>@&lt;name&gt;(?&lt;vrf&gt;.+?)&lt;/name&gt;@</regexp>
          <xpath>/row/vrf/name</xpath>
        </line>
        <line>
          <array/>
          <regexp>@&lt;prefix&gt;(?&lt;object_id&gt;(\d{1,3}\.){3}\d{1,3}/\d{1,2})&lt;/prefix&gt;@</regexp>
          <xpath>/row/prefix</xpath>
        </line>
        <line>
          <array/>
          <regexp>@&lt;value&gt;(?&lt;status&gt;active|reserved|container|deprecated)&lt;/value&gt;@</regexp>
          <xpath>/row/status/value</xpath>
        </line>
        <line>
          <array/>
          <regexp>@&lt;id&gt;(?&lt;id&gt;\d+?)&lt;/id&gt;@</regexp>
          <xpath>/row/id</xpath>
        </line>
      </lines>
    </parser>
    <xpath>/api/ipam/prefixes/</xpath>
  <post_template/>
  </command>
  <command name="UPDATE">
    <operation>{literal}{{/literal}"prefix": "{$params.object_id}", "status": "{$params.status}"{if !empty($params.tenant)}{foreach $NetBox___Tenant item=tenant}{if $tenant.object_id eq $params.tenant}, "tenant": "{$tenant.id}"{/if}{/foreach}{/if}{if !empty($params.site)}{foreach $Netbox___Sites item=site}{if $site.object_id eq $params.site}, "site": "{$site.id}"{/if}{/foreach}{/if}{if !empty($params.vrf)}{foreach $Netbox___VRF item=vrf}{if $vrf.object_id eq $params.vrf}, "vrf": "{$vrf.id}"{/if}{/foreach}{/if}{if !empty($params.vlan_name)}{foreach $Netbox___VLAN item=vlan}{if $vlan.object_id eq $params.vlan_name}, "vlan": "{$vlan.id}"{/if}{/foreach}{/if}
{literal}}{/literal}
</operation>
    <rest>PATCH
</rest>
    <xpath>/api/ipam/prefixes/{$Netbox___Prefix.$object_id.id}/
</xpath>
  </command>
  <command name="CONSTRAINT"/>
  <command name="CREATE">
    <operation>{literal}{{/literal}
	"prefix": "{$params.object_id}"
	, "status": "{$params.status}"
	{if !empty($params.tags)}
		{assign var="temp_tag_array" value=[]}
		{foreach $params.tags item=tag}
			{$temp_tag_array[] = $tag.tag}
		{/foreach}
	, "tags": {$temp_tag_array|@json_encode}
	{/if}
	{if !empty($params.tenant)}
		{foreach $NetBox___Tenant item=tenant}
			{if $tenant.object_id eq $params.tenant}
				, "tenant": "{$tenant.id}"
			{/if}
		{/foreach}
	{/if}
	{if !empty($params.site)}
		{foreach $Netbox___Sites item=site}
			{if $site.object_id eq $params.site}
			, "site": "{$site.id}"
			{/if}
		{/foreach}
	{/if}
	{if !empty($params.vrf)}
		{foreach $Netbox___VRF item=vrf}
			{if $vrf.object_id eq $params.vrf}
				, "vrf": "{$vrf.id}"
			{/if}
		{/foreach}
	{/if}
	{if !empty($params.vlan_name)}
		{foreach $Netbox___VLAN item=vlan}
			{if $vlan.object_id eq $params.vlan_name}
				, "vlan": "{$vlan.id}"
			{/if}
		{/foreach}
	{/if}
{literal}}{/literal}
</operation>
    <rest>POST
</rest>
    <xpath>/api/ipam/prefixes/
</xpath>
