, "tenant": "{$tenant.id}"
{/if}
{/foreach}
{if empty($params.slug)}
, "slug": "{$params.object_id|replace:' ':'-'|lower}"
{else}
, "slug": "{$params.slug}"
{/if}
{if !empty($params.asn)}
, "asn": "{$params.asn}"
{/if}
{literal}}{/literal}
</operation>
    <rest>POST
</rest>
    <xpath>/api/ipam/vrfs/
</xpath>
  </command>
