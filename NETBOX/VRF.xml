<?xml version="1.0" encoding="UTF-8"?>
<ObjectDefinition>
  <information>
    <configType>xml</configType>
    <createTemplateId/>
    <createTemplateObject/>
    <defaultDisplay>false</defaultDisplay>
    <description/>
    <displayField>object_id</displayField>
    <dynamic>false</dynamic>
    <group>NETBOX|IPAM</group>
    <icon>none</icon>
    <importIfMandatoryPresent>false</importIfMandatoryPresent>
    <importonce>false</importonce>
    <importrank>20</importrank>
    <maxInstances>0</maxInstances>
    <name>VRF</name>
    <order>0</order>
    <relatedObjects/>
    <reorderinstances>false</reorderinstances>
    <serviceTaskType/>
    <sortascending>false</sortascending>
    <sortauto>false</sortauto>
    <sortnumerical>false</sortnumerical>
    <sortvariable>param._order</sortvariable>
    <visibility>5</visibility>
  </information>
  <variables frozen="0">
    <variable displayName="ID" displayOrder="0" maxLength="100" name="params.id" numberOfRowMax="0" startIncrement="0" type="String"/>
    <variable displayName="Name" displayOrder="1" maxLength="100" name="params.object_id" numberOfRowMax="0" startIncrement="0" type="String"/>
    <variable displayName="Tenant" displayOrder="2" maxLength="100" name="params.tenant" numberOfRowMax="0" startIncrement="0" type="ObjectRef">
      <classes>
        <class>tenant</class>
      </classes>
    </variable>
    <variable displayName="Route Distinguisher" displayOrder="3" maxLength="100" name="params.rd" numberOfRowMax="0" startIncrement="0" type="String"/>
    <variable default="true" displayName="Shall enforce prefix uniqueness in VRF? " displayOrder="4" maxLength="100" name="params.is_enforce_unique" numberOfRowMax="0" startIncrement="0" type="Boolean"/>
  </variables>
  <example>
    <content>
</content>
  </example>
  <command name="IMPORT">
    <operation>GET</operation>
    <parser>
      <section>
        <regexp>@^\s \s*$@</regexp>
        <xpath>//results/row</xpath>
      </section>
      <lines>
        <line>
          <array/>
          <regexp>@&lt;enforce_unique&gt;(?&lt;is_enforce_unique&gt;true|false)&lt;/enforce_unique&gt;@</regexp>
          <xpath>/row/enforce_unique</xpath>
        </line>
        <line>
          <array/>
          <regexp>@&lt;name&gt;(?&lt;tenant&gt;.+?)&lt;/name&gt;@</regexp>
          <xpath>/row/tenant/name</xpath>
        </line>
        <line>
          <array/>
          <regexp>@&lt;rd&gt;(?&lt;rd&gt;\S+?)&lt;/rd&gt;@</regexp>
          <xpath>/row/rd</xpath>
        </line>
        <line>
          <array/>
          <regexp>@&lt;name&gt;(?&lt;object_id&gt;.*)&lt;/name&gt;@</regexp>
          <xpath>/row/name</xpath>
        </line>
        <line>
          <array/>
          <regexp>@&lt;id&gt;(?&lt;id&gt;\d+?)&lt;/id&gt;@</regexp>
          <xpath>/row/id</xpath>
        </line>
      </lines>
    </parser>
    <xpath>/api/ipam/vrfs/</xpath>
  <post_template/>
  </command>
  <command name="UPDATE">
    <operation>{literal}{{/literal}
"name": "{$params.object_id}"
, "rd": "{$params.rd}"
, "enforce_unique": "{$params.is_enforce_unique}"
{foreach $tenant item=tenant}
{if $tenant.object_id eq $params.tenant}
, "tenant": "{$tenant.id}"
{/if}
{/foreach}
{if empty($params.slug)}
, "slug": "{$params.object_id|replace:' ':'-'|lower}"
{else}
, "slug": "{$params.slug}"
{/if}
{if !empty($params.asn)}
, "asn": "{$params.asn}"
{/if}
{literal}}{/literal}
</operation>
    <rest>PATCH
</rest>
    <xpath>/api/ipam/vrfs/{$VRF.$object_id.id}/
</xpath>
  </command>
  <command name="CONSTRAINT"/>
  <command name="CREATE">
    <operation>{literal}{{/literal}
"name": "{$params.object_id}"
, "rd": "{$params.rd}"
, "enforce_unique": "{$params.is_enforce_unique}"
{foreach $tenant item=tenant}
